        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="companies" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`company-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="company" type="Company" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Company">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Provides information for an organization.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="address" type="Address" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the :ref:`address-object` of the company</xsd:documentation>
