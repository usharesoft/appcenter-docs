                    <xs:element name="certificateUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of uri resources for the :ref:`certificate-object` objects</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

