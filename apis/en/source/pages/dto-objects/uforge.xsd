            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Quota">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides information on quota used to restrict user usage.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="frequency" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">whether the quota should be reset at a certain frequency (only monthly is supported at the moment)</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="monthly"/>
                                <xs:enumeration value="none"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="limit" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the quota limit</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nb" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current number</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="renewalDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the renewal date of the quota</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the quota type</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="appliance"/>
                                <xs:enumeration value="diskusage"/>
                                <xs:enumeration value="generation"/>
                                <xs:enumeration value="scan"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="typeId" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the id of the type</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this quota object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Quotas">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`quota-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="quotas" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`quota-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="quota" type="Quota" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Ranking">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="applianceURI" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="rank" type="xs:int" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Referral">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides information on an invitation's status.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="accepted" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating whether the invitation has been accepted</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="declined" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating whether the invitation has been declined</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="msg" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a message included in the invitation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this object belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="referrer" type="UserProfile" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user creating the invitation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="user" type="UserProfile" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user being invited</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="userEmail" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the email of the user being invited</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="userLogin" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the username of the user being invited</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Referrals">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`referral-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="referrals" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`referral-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="referral" type="Referral" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Repository">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes an operating system repository registered on the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the repository</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this repository object belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this repository object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the operating system(s) of this repository (may contain minor versions)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="url" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the URL endpoint of this repository</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="packagingType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the package type ths repository holds</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="RPM"/>
                                <xs:enumeration value="DEB"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="lastUpdated" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date this repository last received a package update</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="coreRepository" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">
                                boolean indicating when the repository is mandatory for a distribution to function properly.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="officiallySupported" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">
                                DEPRECATED, will be removed in a future release. Use "coreRepository" instead.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionsUriList" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each operating system contained in this repository</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Repositories">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`repository-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="repositories" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`repository-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="repository" type="Repository" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RepositoryUpdate">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Object holding the update information of a :ref:`repository-object`.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this repository update object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="status" type="OpStatus" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current status of the update</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RepositoryUpdates">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`repositoryupdate-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="repositoryUpdates" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`repositoryupdate-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="repositoryUpdate" type="RepositoryUpdate" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Role">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a ``role`` used for Role-Based Access Control (RBAC). A role groups together a list of ``entitlements``
                (see :ref:`entitlement-object`.)

                A role can then be assigned to an user to enforce access rights.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the role name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the role description</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="entitlements" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of entitlements this role contains (see :ref:`entitlement-object`</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="entitlement" type="Entitlement" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Roles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`role-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="roles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`role-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="role" type="Role" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Route">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a networking route. Used as part of a NIC.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="address" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the IP address</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="gateway" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the gateway value</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="metric" type="xs:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the metric value</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="netmask" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the netmask value</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this route is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" type="xs:int"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this route</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Scan">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data of a scan. This is the report information after scanning a live system.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="ScanSummary">
                <xs:sequence>
                    <xs:element name="images" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`image-object` objects that have been generated from this scan report</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="image" type="Image" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="imports" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`scanimport-object` objects that have been imported from this scan report</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="import" type="ScanImport" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="unixServices" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`unixservice-object` objects that represent Unix services on the scanned machine</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="unixService" type="UnixService" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="winServices" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`winservice-object` objects that represent Windows services on the scanned machine</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="winService" type="WinService" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="winApplications" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`winMsiApplication-object` and :ref:`winStoreApplication-object` objects that represent Windows applications on the scanned machine</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="winApplication" type="AbstractWinApplication" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="packages" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`package-object` objects listed in the scan report</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="package" type="ScannedPkg" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScanCompare">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the results of a comparison between two scans.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>

                    <xs:element name="extraFiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`scannedfile-object` files that are not part of any native package on the file system</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="extraFile" type="ScannedFile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="filesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a uri resource to retrieve all the filesystem files reported by the scan process</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="scanAUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the first scan to compare</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="scanBUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the second scan to compare</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="packages" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`package-object` objects listed in the scan report</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="package" type="ScannedPkg" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScanImport">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information of the transformation process from a scan report to an appliance template or a golden image.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="importedObjectName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the imported appliance or golden. This name won't be updated even if the name of the imported object gets changed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="importedObjectUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the URI of the imported appliance or golden. This URI could be deprecated in case the imported object is deleted.</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="importedObjectVersion" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of the imported appliance or golden. This version won't be updated even if the version of the imported object gets changed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="importedObjectDescription" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description of the imported golden only. This description won't be updated even if the description of the imported golden gets changed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="orgUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the Organization where the scan has been imported</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the parent uri of the scan import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="status" type="OpStatus" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the status of the import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the scan import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="importedToGolden" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if the imported object is a golden image. If set to false the imported object is an appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScanImports">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`scanimport-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="scanImports" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`scanimport-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="scanImport" type="ScanImport" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScannedFile">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the meta-data of a file discovered on the file system after a scan process.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="FileEntry">
                <xs:sequence>
                    <xs:element name="deleted" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if the file has been deleted on the source machine</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="inherited" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if this file has already been found, with the same metadata and content, in an other scan of the same scanned instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="selected" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if this file has been selected to be imported as part of the overlay</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uploaded" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if this file has been received as part of the overlay upload</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="changeType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the change type of the file</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="added"/>
                                <xs:enumeration value="deleted"/>
                                <xs:enumeration value="modified"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScannedFiles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`scannedfile-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="scannedFiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`scannedfile-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="scannedFile" type="ScannedFile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="DirectoryStructure">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the meta-data of a directory on the file system.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="id" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the id of the directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="path" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the path of the directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="subDirectories" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of sub directories</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="subDirectory" type="DirectoryStructure" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScannedDirectory">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the meta-data of a directory discovered on the file system after a scan process.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="DirectoryStructure">
                <xs:sequence>
                    <xs:element name="changeType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the change type of the directory</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="ADD"/>
                                <xs:enumeration value="DELETE"/>
                                <xs:enumeration value="CHANGE"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="owner" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the owner of the directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="group" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the group of the directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="permissions" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the permissions of the directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="sumAdded" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the sum of added files in the directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="sumChanged" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the sum of changed files in the directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="sumDeleted" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the sum of deleted files in the directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="files" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of scanned files in the directorie</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="file" type="ScannedFile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScanSync">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Handles synchronization information for files belonging to a scan.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this synchronization object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScanFilesToSynchronize">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of full paths of scanned files to be synchronized from the :ref:`scansync-object` object.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="scanFilesToSynchronize" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of full paths of scanned files to be synchronized</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="fullPath" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ScannedInstance">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Used to group together one or more scan reports from the a running system.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="distribution" type="Distribution" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the operating system detected on the live system (see :ref:`distribution-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="group" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the group name (reserved for future use)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the live system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this scanned instance belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="platformType" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the type of platform the scanned instance is hosted in</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="ramSize" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the size of the virtual memory detected on the source system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="overlayIncluded" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating whether overlay files are included into the scans within this scanned instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="scans" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of scan reports carried out on this live system (see :ref:`scan-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="scan" type="Scan" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="scansUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the scan reports</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="totalScans" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of scans carried out on this live system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this scanned instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScannedInstances">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`scannedinstance-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="scannedInstances" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`scannedinstance-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="scannedInstance" type="ScannedInstance" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="ScannedPkg">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Holds the meta-data of a native package discovered on the file system after a scan process.</xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Pkg">
                <xs:sequence>
                    <xs:element name="deleted" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if the native package has been deleted on the source machine</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="deltaLocation" type="xs:string" minOccurs="0"/>
                    <xs:element name="deltaModified" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if the package has been modified compared to its initial installation state</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the distribution this package is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="filesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the files scanned and associated to this package</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fromRepo" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if the package has been found in any platform registered repositories</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="headers" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the headers (metadata) of the native package</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="inherited" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if this package has already been found, with the same metadata and content, in an other scan of the same scanned instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="mirrorPath" type="xs:string" minOccurs="0"/>
                    <xs:element name="mirrorType" type="xs:string" minOccurs="0"/>
                    <xs:element name="modifiedFiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`scannedfile-object` objects that are modified in this package compared to the initial installation state</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="modifiedFile" type="ScannedFile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="notModifiedFiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`scannedfile-object` objects that have not been modified in this package. Please note: this list will be empty if the package has been detected in one or several of the platform registered repositories</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="notModifiedFile" type="ScannedFile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="postunScripts" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the content of the native package post uninstall scripts</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="preunScripts" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the content of the native package pre uninstall scripts</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="scannedInstanceId" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the id of the parent scanned instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="selected" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean incidating if this package has been marked to be included in the overlay</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tag" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScannedPkgs">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`scannedpackage-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="scannedPackages" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`scannedpackage-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="scannedPackage" type="ScannedPkg" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Scans">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`scan-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="scans" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`scan-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="scan" type="Scan" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScanStage">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data of the scan stage of a migration.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Stage">
                <xs:sequence>
                    <xs:element name="family" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the family type for the operating system of the scan</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="linux"/>
                                <xs:enumeration value="unix"/>
                                <xs:enumeration value="windows"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="scannedInstanceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the scannedInstance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScanStats">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds statistical data on all the scans carried out by an user.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="currGenerated" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the number of scan generations the user still has access to</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="currImported" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the number of appliance templates imported from scans the user still has access to</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="currInstances" type="xs:int" minOccurs="0"/>
            <xs:element name="currOSUsage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">number of times a specific OS has been used when scanning</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="currScans" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the number of scans currently available in the user account</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="totalGenerated" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the number of generations done from a scan</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="totalImported" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the number of appliance imports done from a scan</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="totalInstances" type="xs:int" minOccurs="0"/>
            <xs:element name="totalOSUsage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the number of times a specific distribution has been scanned</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="totalScans" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the number of scans done for a specific user account (counting deleted scans)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ScanSummary">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the summary meta-data of a scan. This is the report information after scanning a live system.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="binariesExtracted" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag determining of the overall has been extracted by the platform</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="binariesReceived" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag determining of the overall has been received by the platform</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="error" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to indicate an error during the scan process</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="errorMsg" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the error message</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="filesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a uri resource to retrieve all the filesystem files reported by the scan process</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="imagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the machine images generated from this scan report</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="osConfig" type="OsConfig" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">`osconfig-object` of the scan</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="importsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the imports</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="goldenImportsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the golden imports</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installProfile" type="InstallProfile" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the install profile of the scanned system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installProfileUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the :ref:`installprofile-object`</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="scanSyncUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the :ref:`scansync-object`</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the scan name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="overlayCrc" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the crc of the scan overlay archive</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="overlayImported" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating whether the overlay has been imported on the platform</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="overlayPath" type="xs:string" minOccurs="0"/>
                    <xs:element name="overlayUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to upload the overlay</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rawPartitionsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to upload the raw partitions (only for Windows source machines)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="packagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the meta data of the scanned packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rejected" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="scanned" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating if the scan report has been received from the scan agent on the source machine</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="scannedInstanceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the parent scanned instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="status" type="OpStatus" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the status of the scan</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="totalFilesAdded" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of extra files found in the scan report</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="totalFilesDeleted" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of files that have been deleted in source machine installed packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="totalFilesModified" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of files that have been modified in the source machine installed packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="totalPackages" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of native packages installed on the source machine</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uncompressedBinarySize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the size of all the overlays</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the scan</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="validated" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="rawScan" type="xs:boolean" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ScanSummaryLight">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds some meta-data information of a scan.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="SourceSummary"/>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Service">
        <xs:complexContent>
            <xs:extension base="PkgGroup">
                <xs:sequence>
                    <xs:element name="bootService" type="xs:boolean" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Services">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="services" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="service" type="Service" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ServiceStatus">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the status of the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="alive" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">determines if the platform is up and running</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current version of the platform</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="serviceLoad" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current service load (max is 100)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SoftwareArtifact">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds information of a native operating system package, file or bootscript.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeUploadObject">
                <xs:sequence>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this software artifact object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tag information</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="binaryUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the binary</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this software artifact belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="extract" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to extract an archive (if the uploaded object is an archive) as part of the generation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="install" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate whether to install the software artifact or just to store it on the filesystem</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="cache" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate whether to put in cache the software artifact or just upload during generation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="active" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate whether the software artifact active or not</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installLocation" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the location to store the software artifact if ``install == false``</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the software artifact</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fullName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the file including the extension</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="origName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the original name of the software artifact</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="artifactBuildDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date the software artifact was built</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="artifactFileDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the file timestamp</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="release" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the release information of the software artifact</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="shortTag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">short tag value (osPackage, softwarefile or bootscript)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">file media type</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of this software artifact</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="arch" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the architecture type for this software artifact (rpm uploaded file or OS Package)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="params" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">silent install parameters to use as part of the installation of the object (for windows only)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="artifactCredAccount" type="ArtifactCredAccount" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the artifact account (see :ref:`artifactcredaccount-object`) this software artifact belong to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="subSoftwareArtifacts" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of sub-software artifact of this software artifact (which represents a folder)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="softwareArtifact" type="SoftwareArtifact" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="subSoftwareArtifactUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of sub-software artifact uris of this software artifact</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentPath" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the parent pathname of this software artifact (depending of its parent software artifact)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="remotePath" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the remote pathname of this software artifact to retrieve from artifact account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SoftwareArtifacts">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`softwareartifact-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="softwareArtifacts" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="softwareArtifact" type="SoftwareArtifact" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SoftwareBundle">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a software bundle (MySoftware or Project).
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="tag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tag information</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="shortTag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the short tag information (mySoftware or project)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version information of this software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description for this software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="release" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the release information for the software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="location" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="defaultInstallLocation" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the default full pathname of where to install the software bundle (if not a native package)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installLocation" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the full pathname of where to install this software bundle item (overrides the ``defaultInstallLocation``)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="restricted" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean highlighting if the Software bundle is restricted</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installedSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the estimated disk size (in bytes) required to install this software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total size of this software bundle (in bytes)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="revision" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the revision number of the software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="bundleRestriction" type="BundleRestriction" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the restriction for this software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="artifactsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the details of software artifacts contained in the software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this software bundle object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="exportUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource used to export this software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="category" type="Category" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the category for this software bundle (project)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="license" type="License" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the license information (see :ref:`license-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logo" type="Logo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the :ref:`logo-object` object for the software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="maintainer" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the maintainer name for this project</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="website" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the website URL of the maintainer</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="softwareArtifactUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each software artifacts in the software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="formatDependent" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean highlighting if the Software bundle is restricted on target format</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="containerTemplatesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri to retrieve container templates created from the software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SoftwareBundles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`softwarebundle-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="softwareBundles" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="softwareBundle" type="SoftwareBundle" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SoftwareBundleExport">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds tracking information on requests to export a software bundle. An export basically
                archives the software bundle meta-data and all relevant files and allows the user to download that archive. The archive
                information can then be version controlled or imported to another UForge platform.

                <p>The software bundle export is an asynchronous job which may take some time. The current status of the export
                    is stored in this object.
                </p>
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="bundleName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this exported software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="bundleVersion" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version information of this exported software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="downloadUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource used to download this exported software bundle (as an archive)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="location" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the full pathname where the exported software bundle (as an archive) is stored locally</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this exported software bundle belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="status" type="OpStatus" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current status of the exported software bundle</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this exported software bundle object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SoftwareBundleImport">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information when a software bundle is being imported to this UForge platform.
                The actual import is done by uploading a software bundle archive via the ``uploadUri``.

                <p>The software bundle import is an asynchronous job which may take some time. The current status of the import is stored
                    in this object.
                </p>
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeUploadObject">
                <xs:sequence>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this object is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="referenceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the software bundle created from this import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uploadUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to upload any files assocaited with this import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="statusUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the current status of the software bundle import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imported" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag indicating that is a real import or a creation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this software bundle import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="SoftwareFile">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds information of a simple file attached to a software bundle.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="SoftwareArtifact">
                <xs:sequence>
                    <xs:element name="ownerGroup" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the string owner:group for this software file to set during the generation of a machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rights" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the rights for this software file (for example 777) to set during the generation of a machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="symlink" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a symbolic link for this software file to set during the generation of a machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SoftwareFiles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`softwarefile-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="softwareFiles" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="softwareFile" type="SoftwareFile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SSHKey">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides information of a public SSH key.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the ssh key</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publicKey" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the public ssh key content</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to this ssh key object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SSHKeys">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`sshkey-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="sshKeys" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`sshkey-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="sshKey" type="SSHKey" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Stage">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data of a stage to perform a pipeline.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="pipelineUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the parent pipeline</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="StatDataPoint">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Generic object holding a statistic data point.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="date" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date of this data point</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distribArch" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the architecture of the operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distribName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the operating system name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distribVersion" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the operating system version</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="endDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the stat timestamp</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="errorMessage" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">error message</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="message" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">message for this data point</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name or tooltip for this data point</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nb" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the value for this data point</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="percentage" type="xs:float" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the percentage</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="startDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the timestamp from where this stat begins</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="total" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total value for this stat</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the type of stat</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="userCrmId" type="xs:string" minOccurs="0"/>
                    <xs:element name="version" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="StatDataPoints">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`statdatapoint-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="statDataPoints" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`statdatapoint-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="StatTicket">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Generic object hodling reference to most of the platform CRUD (create / update / delete) operations.
                StatTicket remains even if the initial attach object gets deleted / updated.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="actionId" type="xs:long" minOccurs="0"/>
                    <xs:element name="actionName" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="added"/>
                                <xs:enumeration value="cancelled"/>
                                <xs:enumeration value="created"/>
                                <xs:enumeration value="deleted"/>
                                <xs:enumeration value="downloaded"/>
                                <xs:enumeration value="failed"/>
                                <xs:enumeration value="imported"/>
                                <xs:enumeration value="purchased"/>
                                <xs:enumeration value="shared"/>
                                <xs:enumeration value="success"/>
                                <xs:enumeration value="updated"/>
                                <xs:enumeration value="uploaded"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="currency" type="xs:string" minOccurs="0"/>
                    <xs:element name="date" type="xs:dateTime" minOccurs="0"/>
                    <xs:element name="description" type="xs:string" minOccurs="0"/>
                    <xs:element name="diskSize" type="xs:long" minOccurs="0"/>
                    <xs:element name="distributionId" type="xs:long" minOccurs="0"/>
                    <xs:element name="distributionName" type="xs:string" minOccurs="0"/>
                    <xs:element name="formatId" type="xs:long" minOccurs="0"/>
                    <xs:element name="formatName" type="xs:string" minOccurs="0"/>
                    <xs:element name="fromObjectId" type="xs:long" minOccurs="0"/>
                    <xs:element name="fromObjectName" type="xs:string" minOccurs="0"/>
                    <xs:element name="fromObjectType" type="xs:string" minOccurs="0"/>
                    <xs:element name="objectId" type="xs:long" minOccurs="0"/>
                    <xs:element name="objectName" type="xs:string" minOccurs="0"/>
                    <xs:element name="objectStatus" type="xs:string" minOccurs="0"/>
                    <xs:element name="objectType" type="xs:string" minOccurs="0"/>
                    <xs:element name="orgId" type="xs:long" minOccurs="0"/>
                    <xs:element name="price" type="xs:float" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="userId" type="xs:long" minOccurs="0"/>
                    <xs:element name="userLoginName" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="StatTickets">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`statticket-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="statTickets" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`statticket-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="statTicket" type="StatTicket" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SubscriptionProfile">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes access and permission information used when creating new users on the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="active" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to indicate whether this subscription profile can be used</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="admins" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of administrators that can use this subscription profile when creating a new user (see :ref:`user-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="admin" type="UserProfile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="adminsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the administrators that can use this subscription profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="code" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the subscription profile code to use when creating a user (this subscription profile will be used)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description of this subscription profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distribsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the operating systems that the user will be given access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributions" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of operating systems that the user will be given access to (see :ref:`distribution-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="distribution" type="Distribution" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="formats" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of primitive formats that the user will be given access to (see :ref:`imageformat-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="format" type="ImageFormat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="formatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the primitive formats that the user will be given access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetPlatforms" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of target platforms that the user will be given access to (see :ref:`targetplatform-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="targetPlatform" type="TargetPlatform" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="targetPlatformsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the target platforms that the user will be given access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetFormats" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of target formats that the user will be given access to (see :ref:`targetformat-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="targetFormat" type="TargetFormat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="targetFormatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the target formats that the user will be given access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this subscription profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="orgUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the organization this subscription profile is registered to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="quotas" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of quotas that the user will be given (see :ref:`quota-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="quota" type="Quota" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="quotasUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the quota information that the user will be given</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="roles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of roles that the user will be given to (see :ref:`role-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="role" type="Role" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="rolesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the roles that the user will be given</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this subscription profile object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SubscriptionProfiles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`subscriptionprofile-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="subscriptionProfiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`subscriptionprofile-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="subscriptionProfile" type="SubscriptionProfile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="SourceSummary" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides source information.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Tag">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides tagging information.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the public tag name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this tag object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="TargetFormat">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds information that extends a primitive :ref:`imageformat-object`. This allows administrators to customize and
                rebrand machine image formats.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this format</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logo" type="Logo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the logo of this format (see :ref:`logo-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="format" type="ImageFormat" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the primitive format this target platform is based on</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="category" type="Category" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the category for this target format</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the target format type</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="cloud"/>
                                <xs:enumeration value="virtual"/>
                                <xs:enumeration value="container"/>
                                <xs:enumeration value="physical"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="credAccountType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the cloud account type this target format is mapped to</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="aws"/>
                                <xs:enumeration value="outscale"/>
                                <xs:enumeration value="azure"/>
                                <xs:enumeration value="google"/>
                                <xs:enumeration value="cloudstack"/>
                                <xs:enumeration value="openstack"/>
                                <xs:enumeration value="openshift"/>
                                <xs:enumeration value="vclouddirector"/>
                                <xs:enumeration value="vsphere"/>
                                <xs:enumeration value="k5"/>
                                <xs:enumeration value="docker"/>
                                <xs:enumeration value="oracle"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="targetPlatformsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the target platforms where this target format is used</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetPlatformsUriList" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for the target platform objects where this target format is used</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="credInfos" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the end-user information to get the necessary credentials to generate a machine image to this particular TargetFormat</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imageInfos" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the end-user information that could be provided before a user generates a machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publishInfos" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the end-user information that could be provided before a user publishes a machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="access" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating whether the user has access to the TargetFormat or not</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="active" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicated whether the TargetFormat is availble in the Org the user is registered to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="preselected" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this target format object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="TargetFormats">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`targetformat-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="targetFormats" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`targetformat-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="targetFormat" type="TargetFormat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="TargetPlatform">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds information on a target cloud platform that extends one of the primitive cloud environments supported by the
                platform. This allows administrators to customize target cloud environments. A target platform may include one
                or more :ref:`targetformat-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this target platform</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logo" type="Logo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the logo of this target platform (see :ref:`logo-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the primitive type for this target platform</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="aws"/>
                                <xs:enumeration value="outscale"/>
                                <xs:enumeration value="azure"/>
                                <xs:enumeration value="google"/>
                                <xs:enumeration value="cloudstack"/>
                                <xs:enumeration value="openstack"/>
                                <xs:enumeration value="vclouddirector"/>
                                <xs:enumeration value="vsphere"/>
                                <xs:enumeration value="k5"/>
                                <xs:enumeration value="docker"/>
                                <xs:enumeration value="oracle"/>
                                <xs:enumeration value="openshift"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="targetFormatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the target formats this target platform contains</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetFormatsUriList" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each target format this target platform contains</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="accountInfos" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the end-user information that could be displayed to help the user get the necessary credentials information (cloud platform api keys, etc.)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="access" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating whether the user has access to the TargetFormat or not</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="active" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicated whether the TargetFormat is availble in the Org the user is registered to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource for this target platform object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="TargetPlatforms">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`targetplatform-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="targetPlatforms" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`targetplatform-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="targetPlatform" type="TargetPlatform" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="UForgeError">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds information on a platform error.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="localizedErrorMsg" type="LocalizedMessage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the error message (supports i18n)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="requestMethod" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the HTTP request method used in the request</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="requestUri" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the HTTP uri resource used in the request</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="statusCode" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the HTTP status code</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="timestamp" type="xs:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the timestamp the request was received by the platform</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="UForgeObject" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds generic information used by platform objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="created" type="xs:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the date the appliance template is created</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="dbId" type="xs:long" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the database id of the object</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="digest" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the digest value (used for etag)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="lastModified" type="xs:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the last modified date of this object</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="UForgeObjects" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`uforgeobject-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="total" type="xs:long" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the total number of objects</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="UForgeUploadObject" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds generic information used by platform objects that can be uploaded
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="md5sum" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the md5sum of the uploaded object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="mimeType" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the mime type of the file uploaded</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="sha256sum" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the sha256 value of the uploaded object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the size (in bytes) of the uploaded object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="status" type="OpStatus" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the upload status (see :ref:`status-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uploaded" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean to indicate if the file has been uploaded</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uploadedTime" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the timestamp the upload was completed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="UriList">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of URI resources.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="uri" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="User">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes an user of the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UserProfile">
                <xs:sequence>
                    <xs:element name="address" type="Address" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's address</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="admin" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating if the user is an administrator</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="adminOrgsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get the list of Org this user is an administrator of</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="adminOrgUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of Org URI where this user is an administrator</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="apiKeysQuota" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of API keys this user can generate</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applianceUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each appliance template this user is managing</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseApiKeysUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's api keys</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseAppliancesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's appliance templates</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseBlueprintsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseCredAccountsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's cloud accounts</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseDeploymentsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's deployments</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseDistributionsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the operating systems the user has access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseFormatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the primitive formats the user has access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseTargetPlatformsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the target platforms the user has access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseTargetFormatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the target formats the user has access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseGalleriesUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="baseGalleryOrgsUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="baseImagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's machine images</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseExportsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's exported appliance templates</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseImportsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's imported appliance templates</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="mySoftwareImportsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's imported software bundles</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseMySoftwareLibraryUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's software library (containing private software components)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseOrgsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the organizations the user is a member of</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseProjectsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the project catalog software components</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="basePublishImagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's published machine images</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseRankingsUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="baseRolesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's roles</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseTagsUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="baseWorkspacesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the workspaces the user has access to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="creationCode" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the code used for creating the user (subscription profile code, see :ref:`subscriptionprofile-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="credAccountUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each cloud account the user has</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="artifactAccountsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's artifact accounts</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="dob" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's date of birth</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="gender" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's gender</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="homePhone" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's home phone number</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="instancesUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="lang" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's preferred language</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="migrationsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the migrations for the user (see :ref:`migration-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="mobilePhone" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's mobile phone number</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="mySoftwareLibraryUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each software component the user has in their software library</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's first name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="officePhone" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's office phone number</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="orgUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each organization this user is a member of</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="password" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's password</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="productAccess" type="ProductAccess" minOccurs="0"/>
                    <xs:element name="projectUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each software component in the project catalog</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="promoCode" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the promotion code used when creating the user account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publishGalleries" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="gallery" type="Gallery" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="quotasUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the user's quota information</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rankings" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="ranking" type="Ranking" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="roles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of roles assigned to the user (see :ref:`role-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="role" type="Role" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="scannedInstancesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the scanned instances for the user (see :ref:`scannedinstance-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="scansUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the scans for the user (see :ref:`scan-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="sshKeys" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of public ssh keys for the user (see :ref:`sshkey-object`</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="sshKey" type="SSHKey" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="sshKeysUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the publish ssh keys for the user</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="statsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the user statistics</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tags" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="tag" type="Tag" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this user object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="usageUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the platform usage for the user</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="vAppAccess" type="VAppAccess" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="UserGroup">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="admin" type="User" minOccurs="0"/>
                    <xs:element name="members" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="member" type="User" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="membersUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="partner" type="xs:int" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="UserGroups">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="userGroups" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="userGroup" type="UserGroup" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="UserProfile">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes an user's profile
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="loginName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user login name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="active" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating whether the user account is active</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="company" type="Company" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the company information for this user</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="email" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's email address</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="firstName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's first name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="surname" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's surname</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="jobTitle" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's job title</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="photo" type="Logo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's photo / avatar</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="website" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's website</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="UserProfiles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`userprofile-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="userProfiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`userprofile-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="userProfile" type="UserProfile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Users">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`user-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="users" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`user-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="user" type="User" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="UserStat">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds statistical data for a user of the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">statistic type (APPLIANCE | FORMAT_APPLIANCE | FORMAT_SCAN | GENERATION_APPLIANCE | GENERATION_SCAN | OS_APPLIANCE | OS_SCAN | SCAN | SOFTWARE_BUNDLE)</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="APPLIANCE"/>
                                <xs:enumeration value="FORMAT_APPLIANCE"/>
                                <xs:enumeration value="FORMAT_SCAN"/>
                                <xs:enumeration value="GENERATION_APPLIANCE"/>
                                <xs:enumeration value="GENERATION_SCAN"/>
                                <xs:enumeration value="OS_APPLIANCE"/>
                                <xs:enumeration value="OS_SCAN"/>
                                <xs:enumeration value="SCAN"/>
                                <xs:enumeration value="SOFTWARE_BUNDLE"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>

                    <xs:element name="currNb" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">current number of items</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="dataPoints" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">
                                a list of :ref:`statdatapoint-object` objects.
                            </xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="dataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="UserStats">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`userstat-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="stats" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="stat" type="UserStat" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="VAppAccess">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="allowed" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="cryptAlgo" type="xs:int" minOccurs="0"/>
                    <xs:element name="expires" type="xs:dateTime" minOccurs="0"/>
                    <xs:element name="licenseContents" type="xs:string" minOccurs="0"/>
                    <xs:element name="licenseKey" type="xs:string" minOccurs="0"/>
                    <xs:element name="machineId" type="xs:string" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="VirtualImagesRepositories">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="virtualImagesRepositories" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="virtualImagesRepository" type="VirtualImagesRepository" minOccurs="0"
                                            maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="VirtualImagesRepository">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="provider" type="CloudProvider"/>
                    <xs:element name="images" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="image" minOccurs="0" maxOccurs="unbounded">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="tenant" type="xs:string"/>
                                            <xs:element name="imageId" type="xs:string"/>
                                            <xs:element name="distribution">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="name" type="xs:string"/>
                                                        <xs:element name="platform" type="xs:string"/>
                                                        <xs:element name="version" type="xs:string"/>
                                                        <xs:element name="release" type="xs:string"/>
                                                        <xs:element name="edition" type="xs:string"/>
                                                        <xs:element name="architecture" type="xs:string"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="VmwVSphereResources">
        <xs:complexContent>
            <xs:extension base="CloudResources">
                <xs:sequence>
                    <xs:element name="datacentres" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="datacentre" minOccurs="0" maxOccurs="unbounded">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="hosts" minOccurs="0">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="host" minOccurs="0" maxOccurs="unbounded">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element name="datastoresNames" minOccurs="0">
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                    <xs:element name="name" type="xs:string" minOccurs="0"/>
                                                                    <xs:element name="networksNames" minOccurs="0">
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element name="name" type="xs:string" minOccurs="0"/>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="VoteStat">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides information on a review rating
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="loginName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the login name of the user sending the rating</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this object belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rating" type="xs:float" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the rating value</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="user" type="User" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user information providing the rating (see :ref:`user-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="WindowsProfile">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a group of native applications and services from a windows operating system. This profile is
                created by a platform administrator or used in an appliance template. These templates are used by
                users of the platform to easily create operating profiles that are part of an appliance template.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="DistribProfile">
                <xs:sequence>
                    <xs:element name="edition" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the edition of the operating system for this os profile</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="DATACENTER"/>
                                <xs:enumeration value="STANDARD"/>
                                <xs:enumeration value="ENTERPRISE"/>
                                <xs:enumeration value="WEBSERVER"/>
                                <xs:enumeration value="UNKNOWN"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="importedGolden" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating if this os profile was created from an imported scan</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionTable" type="PartitionTable" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the partition table of the os profile (see :ref:`partitiontable-object`).</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the type of the operating system for this os profile</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="FULL"/>
                                <xs:enumeration value="CORE"/>
                                <xs:enumeration value="UNKNOWN"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="language" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the language of the attached golden machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="servicesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the services contained in this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applicationsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the applications contained in this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="WindowsProfiles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`windowsprofile-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="windowsProfiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`windowsprofile-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="windowsProfile" type="WindowsProfile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="WinMsiApplication">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides information of a Windows Msi Application
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="AbstractWinApplication">
                <xs:sequence>
                    <xs:element name="publisher" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">The publisher of the msi application</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installLocation" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the location the msi application is installed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="estimatedSizeInKB" type="xs:integer" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the estimated size of the msi application</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="WinMsiApplications">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`winMsiApplication-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="winMsiApplications" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`winMsiApplication-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="winMsiApplication" type="WinMsiApplication" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="WinStoreApplication">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides information of a Windows Store Application
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="AbstractWinApplication">
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="WinStoreApplications">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`winStoreApplication-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="winStoreApplications" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`winStoreApplication-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="winStoreApplication" type="WinStoreApplication" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="WinService">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides information of a Windows Service
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="serviceName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">The name of the service</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="displayName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the human readable name of the service</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description of the service</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="startupType" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="SERVICE_AUTO_START"/>
                                <xs:enumeration value="SERVICE_DEMAND_START"/>
                                <xs:enumeration value="SERVICE_DISABLED"/>
                                <xs:enumeration value="SERVICE_STARTUPTYPE_UNKNOWN"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the resource uri of the parent :ref:`windowsprofile-object` object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the resource uri of the service</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="UnixService">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides information of an Unix Service
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="serviceManager" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">The service manager</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="serviceName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">The name of the service</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="serviceStatus" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">The status of the service</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="WinServices">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`winservice-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="winServices" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`winservice-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="winService" type="WinService" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Workspace">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a workspace used to share appliance templates between users.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="commentsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the comments posted to this workspace</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="membershipsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the members of this workspace</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="myMembership" type="Membership" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the membership information on the user sending the request</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the workspace</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbTemplates" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of shared appliance templates in the workspace</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="orgUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the organization where this workspace is created</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="templatesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the shared appliance templates</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this workspace</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Workspaces">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`workspace-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="workspaces" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`workspace-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="workspace" type="Workspace" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>
