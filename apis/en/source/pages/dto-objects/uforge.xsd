                <xs:sequence>
                    <xs:element name="namespace" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the namespace to use when registering the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="repositoryName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name to use when registering the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tagName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tag name to use when registering the machine image</xsd:documentation>
