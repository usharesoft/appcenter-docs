    <xs:complexType name="AbstractWinApplication" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the basic information of a Windows application
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="displayName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the application</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of the application</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="architecture" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the architecture of the application</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="x64"/>
                                <xs:enumeration value="x86"/>
                                <xs:enumeration value="neutral"/>
                                <xs:enumeration value="arm"/>
                                <xs:enumeration value="unknown"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the resource uri of the parent :ref:`windowsprofile-object` object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the resource uri of the application</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AbstractWinApplications">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`abstractwinapplication-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="abstractWinApplications" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="abstractWinApplication" type="AbstractWinApplication" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Address">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the address information of an organization or user.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="country" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the country name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="region_state" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the state or region name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="street" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the street name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="town" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the town or city name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="zipcode" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the zipcode or postal address</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="APIKeyPair">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the key pair information used as an alternative way to using basic authorization for
                HTTP requests.

                The public key is inserted as a query in the request URI. The secret key is then used to encode the entire URI
                to create a signature using HMAC_SHA1. This signature is then added to the end of the request URI.

                These keys are created automatically by the platform. For more information, refer to :ref:`apikey-api-resources`.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">name or tag of the API key pair</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">description of the API key pair</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publicKey" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the public key string</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="secretKey" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the private key string</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the resource uri of the `APIKeyPair` object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="APIKeyPairs">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`apikeypair-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="apiKeyPairs" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`apikeypair-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="apiKeyPair" type="APIKeyPair" minOccurs="0" maxOccurs="unbounded">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Appliance">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data information of a software stack (known as an appliance template).
                It consists of five layers, namely:

                - an Install Profile - specific information for the first time the image boots

                - an OS Profile - a list of operating system packages. Each operating system within the organization provides one or more standard OS profiles to choose from when creating the OS Profile of the appliance template. It is also possible to create custom OS profiles

                - Projects - a list of Project software components chosen from the Organization´s Project Catalog

                - My Software - a list of software components chosen from the User´s private “Software Library”

                - Configuration - configuration information including boot scripts and/or other software components to manage the image after provisioning

                Appliance templates are then used to generate ``machine images``.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="uuid" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uuid of the object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="applianceExports" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`applianceexport-object` objects holding meta-data information for each time
                                the appliance has been exported.
                            </xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="applianceExport" type="ApplianceExport" minOccurs="0" maxOccurs="unbounded">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="archName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the operating system architecture this appliance is based on</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseCloneUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource used to clone this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseImagesStatusUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve machine images status</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseImagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve machine images generated from this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseMySoftwareUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve the software components added to this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseOasPackageUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve any ``UForge Studio`` package added to this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseOsProfileUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve the ``OS Profile`` for this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseProjectsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve any projects added to this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="basePublishImagesStatusUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve published machine image status</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="basePublishImagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve published machine images from this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseTemplatesStatusUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve the status for each time this appliance has been shared (to workspaces)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseTemplatesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to retrieve all the shared templates (to workspaces)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="bootScripts" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`bootscript-object` objects added to this appliance</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="bootScript" type="BootScript" minOccurs="0" maxOccurs="unbounded">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="bootScriptsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to get the boot scripts added to this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="bootScriptUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of uris for the boots scripts added to this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="confMgmtPlatforms" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="confMgmtPlatform" type="ConfMgmtPlatform" minOccurs="0" maxOccurs="unbounded">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="confMgmtPlatformsUri" type="xs:anyURI" minOccurs="0">
                    </xs:element>
                    <xs:element name="confMgmtPlatformUris" type="UriList" minOccurs="0">
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a description of the appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the operating system this appliance is based on (includes name and version)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to get the details of the operating system this appliance is based on</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="exportsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to get all the export objects for this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imageUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of uris for the machine images generated from this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imported" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine if this appliance is an import (e.g. from a ``scan``)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installedSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">estimated disk size required to install the appliance's software</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installProfile" type="InstallProfile" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the :ref:`installprofile-object` object for this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installProfileUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource to get the ``install profile`` of the appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="lastPkgUpdate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the ``os profile`` packages update date (this date is used to calculate if any new package updates are available</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logo" type="Logo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the :ref:`logo-object` object for the appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="mySoftwareList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`mysoftware-object` objects describing the software components added to this appliance</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="mySoftware" type="MySoftware" minOccurs="0" maxOccurs="unbounded">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="mySoftwareUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of uris for the software components added to this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="nbSecurityUpdates" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of security operating system packages available for update</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="nbUpdates" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of native package updates available for update</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="oasPackage" type="OASPkg" minOccurs="0"/>

                    <xs:element name="oasPackageUri" type="xs:anyURI" minOccurs="0"/>

                    <xs:element name="orgUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the organization this appliance belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="osProfile" type="DistribProfile" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the :ref:`distribprofile-object` object describing the operating system profile of this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="osProfileUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource for the operating system profile of this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="parentApplianceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent appliance. This is set if this appliance is a clone of another appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="parentTemplateUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of an parent appliance template. This is set of this appliance is a result of importing an appliance (e.g. from a workspace)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="projects" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`project-object` objects added to this appliance</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="project" type="Project" minOccurs="0" maxOccurs="unbounded">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="projectUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of uris for the projects added to this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publishImages" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`publishimage-object` objects for the machine images that have been registered to
                                one or more cloud platforms
                            </xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="publishImage" type="PublishImage" minOccurs="0" maxOccurs="unbounded">
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="publishImageUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of uris for the machine images that have been registered to one or more cloud platforms</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>

                    <xs:element name="revision" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the revision number of this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="shared" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean highlighting this appliance has been shared to at least one workspace</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the size (in bytes) of all the packages and software added to this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a tag for this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of this appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="ApplianceExport">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds tracking information on requests to export an appliance. An export basically archives the appliance meta-data and all relevant software and allows the user to download that archive. The archive information can then be version controlled or imported
                to another UForge platform.

                The appliance export is an asynchronous job which may take some time. The current status of the export is stored in this object.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="applianceName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the exported appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applianceVersion" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of the exported appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="downloadUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri allowing the archive to be downloaded</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="downloaded" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">flag to determine if there has been at least one request to download the export archive</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <!-- hack, this should not be in uforge.xsd but in a xsd dedicated to event bus objects -->
                    <xs:element name="location" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the exported appliance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="status" type="OpStatus" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the :ref:`status-object` object providing the current status of the export</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this appliance export</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ApplianceExports">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`applianceExport-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="applianceExports" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`applianceExport-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="applianceExport" type="ApplianceExport" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ApplianceImport">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information when an appliance is being imported to this UForge platform.
                The actual import is done by uploading an appliance archive via the ``uploadUri``.

                The appliance import is an asynchronous job which may take some time. The current status of the import is stored in this object.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeUploadObject">
                <xs:sequence>
                    <xs:element name="useMajor" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to instruct the import process to use only the major version of the operating system (e.g. rather than CentOS 6.2, CentOS 6 will be used)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="reuseBundles" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to instruct the import process to re-use any software components already available in the platform rather than re-importing these software components into the user's software library</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="force_rw" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to instruct the import process to overwrite any appliance template with the same name and version already existing on the platform (potentially from a previous import)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imported" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag indicating whether the import is complete</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this object is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="referenceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the appliance template created from this import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uploadUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to upload any artifacts assocaited with this import (software components etc)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this appliance import</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ApplianceImports">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`applianceImport-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="applianceImports" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`applianceImport-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="applianceImport" type="ApplianceImport" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Appliances">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`appliance-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="appliances" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`appliance-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="appliance" type="Appliance" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ApplianceStats">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the statistic information of all the appliance templates for a user.
                This includes:

                - the number of appliance templates

                - the number of machine images generated and a breakdown on the format types used

                - the breakdown on the operating systems used

                - the number appliance templates shared (e.g. to workspaces)
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="curr" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the value</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="currOSUsage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">list of :ref:`statdatapoint-object` for the breakdown on operating system types currently being used for the user's appliance templates</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="currShared" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">number of appliance templates currently being shared (e.g. in a workspace)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="currSharedPublic" type="xs:int" minOccurs="0"/>
            <xs:element name="currUsage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of :ref:`statdatapoint-object` objects holding the statistic history</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="total" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the total value (over time regardless if the object in question has been deleted)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="totalOSUsage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">list of :ref:`statdatapoint-object` for the breakdown on operating system types that have been used for the user's appliance templates</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="totalShared" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">number of appliance templates that have been shared (e.g. in a workspace)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="totalSharedPublic" type="xs:int" minOccurs="0"/>
            <xs:element name="totalUsage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of :ref:`statdatapoint-object` objects holding the statistic history (over time regardless if the object in question has been deleted)</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="ApplianceSummary">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds some meta-data information of an appliance template.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="SourceSummary">
                <xs:sequence>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of the appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ArtifactCredAccount">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds artifact account information for a software artifact. These credentials allow the platform
                to communicate with the remote repository to fetch to add files for a software or project.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this artifact account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this artifact account is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the artifact account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="host" type="xs:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the hostname of the artifact account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="login" type="xs:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the login (optional) to connect to the artifact account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="password" type="xs:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the password (optional) to connect to the artifact account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the type of the artifact account, for example ``ftp``</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="http"/>
                                <xs:enumeration value="https"/>
                                <xs:enumeration value="ftp"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="port" type="xs:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the port of the artifact account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="softwareArtifactsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve artifacts from the artifact account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ArtifactCredAccounts">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`artifactcredaccount-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="artifactCredAccounts" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="artifactCredAccount" type="ArtifactCredAccount" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AwsResources">
        <xs:complexContent>
            <xs:extension base="CloudResources">
                <xs:sequence>
                    <xs:element name="ec2Service" minOccurs="0">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="AwsService">
                                    <xs:sequence>
                                        <xs:element name="amis" minOccurs="0">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="ami" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="id" type="xs:string"/>
                                                                <xs:element name="name" type="xs:string" minOccurs="0"/>
                                                                <xs:element name="publicImage" type="xs:boolean" minOccurs="0"/>
                                                                <xs:element name="ramDiskId" type="xs:string" minOccurs="0"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="regions" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="region" minOccurs="0" maxOccurs="unbounded">
                                    <xs:complexType>
                                        <xs:complexContent>
                                            <xs:extension base="AwsService">
                                                <xs:sequence>
                                                    <xs:element name="localisation" type="xs:string"/>
                                                </xs:sequence>
                                            </xs:extension>
                                        </xs:complexContent>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="s3Service" minOccurs="0">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="AwsService">
                                    <xs:sequence>
                                        <xs:element name="buckets" minOccurs="0">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="bucket" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:complexContent>
                                                                <xs:extension base="AwsService">
                                                                    <xs:sequence>
                                                                        <xs:element name="name" type="xs:string" minOccurs="0"/>
                                                                        <xs:element name="owner" type="xs:string" minOccurs="0"/>
                                                                        <xs:element name="permission" minOccurs="0">
                                                                            <xs:complexType>
                                                                                <xs:sequence>
                                                                                    <xs:element name="read" type="xs:boolean"/>
                                                                                    <xs:element name="readACP" type="xs:boolean"/>
                                                                                    <xs:element name="write" type="xs:boolean"/>
                                                                                    <xs:element name="writeACP" type="xs:boolean"/>
                                                                                </xs:sequence>
                                                                            </xs:complexType>
                                                                        </xs:element>
                                                                    </xs:sequence>
                                                                </xs:extension>
                                                            </xs:complexContent>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AwsService" abstract="true">
        <xs:sequence>
            <xs:element name="RegionId" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="UForgeURIAbleObject">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Blueprint">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a blueprint.
                A blueprint modelizes a set of :ref:`blueprintService-object` and the information required to deploy these services
                on different nodes in a container or cloud environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of the blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="revision" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the revision of the blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description of the blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="yaml" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the blueprint yaml content</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="deployUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to deploy the blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="blueprintServices">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="blueprintService" type="BlueprintService" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="predeployUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve BlueprintPredeploy</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="prepareUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to launch a pipeline based on this blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="pipelines">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of Pipelines used to prepare this blueprint</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="pipeline" type="Pipeline" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Blueprints">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="blueprints" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="blueprint" type="Blueprint" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BlueprintDeployment">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the information required to deploy a blueprint.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the blueprint deployment name used for this deployment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="credAccountUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the cloud account uri used for this deployment (see :ref:`credaccount-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BlueprintDeploymentAws">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the information required to deploy a blueprint to the Amazon Web Services (AWS) environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="BlueprintDeployment">
                <xs:sequence>
                    <xs:element name="region" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the region where this machine image is registered</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BlueprintPipeline" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the general information on the preparation of a blueprint to a target cloud environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Pipeline">
                <xs:sequence>
                    <xs:element name="blueprintUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the blueprint uri related to this preparation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="credAccountUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the cloud account used by this preparation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BlueprintPipelineAws">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information on the preparation of a blueprint to the Amazon Web Services (AWS) environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="BlueprintPipeline">
                <xs:sequence>
                    <xs:element name="region" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the region where the blueprint is prepared</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BlueprintPipelines">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="blueprintPipelines" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="blueprintPipeline" type="BlueprintPipeline" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BlueprintPredeploy">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the information required to know the status of the appliances related to a blueprint.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="readyAppliances">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of appliances that are ready</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="readyAppliances" type="Appliance" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="publicationOngoingAppliances">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of appliances having a publication ongoing</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="publicationOngoingAppliances" type="Appliance" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="nonPublishedAppliances">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of appliances that are not published</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="nonPublishedAppliances" type="Appliance" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="nonGeneratedAppliances">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of appliances that are not generated</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="nonGeneratedAppliances" type="Appliance" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="generationOngoingAppliances">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of appliances having an image generation ongoing</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="generationOngoingAppliances" type="Appliance" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="interactiveProfileAppliances">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of appliances having an interactive profile</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="interactiveProfileAppliances" type="Appliance" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BlueprintService">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a blueprint service.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="serviceId" type="xs:string">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the id of this blueprint service. It can be used in ochestrator configuration, see blueprints example provided in UForge user guide documentation for details.</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="applianceSummary" type="ApplianceSummary">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the appliance summary used to generate this blueprint service node</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BlueprintServices">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="blueprintServices" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="blueprintService" type="BlueprintService" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BlueprintsSummaries">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="blueprintsSummaries" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="BlueprintSummary" type="BlueprintSummary" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BlueprintSummary">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the main meta-data information used to identify a specific :ref:`blueprint-object` template.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="SourceSummary">
                <xs:sequence>
                    <xs:element name="version" type="xs:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of the blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="revision" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the revision of the blueprint</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BootScript">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides all the information of a boot script file that is executed on initial boot
                of a machine instance. The boot script can be executed only once on the first boot of the machine instance, or
                executed on every reboot of the machine instance.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="SoftwareArtifact">
                <xs:sequence>
                    <xs:element name="bootOrder" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the boot order number (starting at 0)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="bootType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the type of the boot script. The boot script can either be executed once during initial provisioning: `firstboot`; or executed every time the instance is rebooted: `everyboot`</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="everyboot"/>
                                <xs:enumeration value="firstboot"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BootScripts">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`bootscript-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="bootScripts" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`bootscript-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="bootScript" type="BootScript" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BtrfsStoragePool">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a btrfs storage pool in a partitioning table. A volume group creates a pool of disk space from a
                collection of disks or partitions. This volume group can then be partitioned via a logical volume.

                .. note:: Only partitions and LVM logical volumes of type btrfs can be added to a btrfs storage pool
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="dataRaid" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the data RAID level of this btrfs storage pool</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="label" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the label of this btrfs storage pool</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="metadataRaid" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the metadata RAID level of this btrfs storage pool</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionTableUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the partition table object (see :ref:`partitiontable-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resources for each partition this btrfs storage pool uses</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each partition this btrfs storage pool uses</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="raidType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the RAID type for data and metadata</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="dup"/>
                                <xs:enumeration value="single"/>
                                <xs:enumeration value="raid0"/>
                                <xs:enumeration value="raid1"/>
                                <xs:enumeration value="raid5"/>
                                <xs:enumeration value="raid6"/>
                                <xs:enumeration value="raid10"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this btrfs storage pool</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BtrfsStoragePools">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`btrfsstoragepool-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="btrfsStoragePools" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`btrfsstoragepool-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="btrfsStoragePool" type="BtrfsStoragePool" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BtrfsSubvolume">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a btrfs subvolume in a partitioning table. A btrfs subvolume describes the way a ``btrfs storage pool`` should
                be partitioned.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="btrfsStoragePoolUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the btrfs storage pool this subvolume uses</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="compress" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the compression method of this btrfs storage pool</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="compressType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the compression method type</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="no"/>
                                <xs:enumeration value="lzo"/>
                                <xs:enumeration value="zlib"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="mpoint" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the mount point if no subvolumes</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionTableUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the partition table object (see :ref:`partitiontable-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this btrfs subvolumes object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="subvolumes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of subvolumes (partitions), see :ref:`partition-object`</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="subvolume" type="Partition" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="subvolumesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the list of subvolumes (partitions)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="subvolumeUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources of the subvolume objects (partitions)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BtrfsSubvolumes">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`btrfssubvolume-object` objects.
            </xsd:documentation>
