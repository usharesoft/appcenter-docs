                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="accountNumber" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the AWS account number to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="keyPairName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the key pair name (for EBS) to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="secretAccessKeyId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the secret access key to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CredAccountAzure">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds specific account information for the Microsoft Azure cloud platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="CredAccount">
                <xs:sequence>
                    <xs:element name="tenantId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tenant id to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="subscriptionId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the subscription id to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applicationId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the application id to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applicationKey" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the application key to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CredAccountCert" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Extends the basic :ref:`credaccount-object` adding certificate information
                required for a cloud account
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="CredAccount">
                <xs:sequence>
