            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="computeEndPoint" type="xs:string" minOccurs="0">
                        <xsd:annotation>
