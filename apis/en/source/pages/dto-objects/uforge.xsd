            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="keystoneDomain" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the keystone domain to use when registering the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="keystoneProject" type="xs:string" minOccurs="0">
