                                                                                        </xs:complexType>
                                                                                    </xs:element>
                                                                                </xs:sequence>
                                                                            </xs:extension>
                                                                        </xs:complexContent>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="serviceOfferingId" type="xs:string"/>
                                                    <xs:element name="serviceOfferingName" type="xs:string"/>
                                                    <xs:element name="status" type="xs:string"/>
                                                    <xs:element name="templateId" type="xs:string"/>
                                                    <xs:element name="templateName" type="xs:string"/>
                                                    <xs:element name="zoneId" type="xs:string"/>
                                                    <xs:element name="zoneName" type="xs:string"/>
                                                </xs:sequence>
                                            </xs:extension>
                                        </xs:complexContent>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="zones" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="zone" minOccurs="0" maxOccurs="unbounded">
                                    <xs:complexType>
                                        <xs:complexContent>
                                            <xs:extension base="CloudstackEntity">
                                                <xs:sequence>
                                                    <xs:element name="allocationEnabled" type="xs:boolean"/>
                                                    <xs:element name="description" type="xs:string"/>
                                                    <xs:element name="dhcpProvider" type="xs:string"/>
                                                    <xs:element name="dns1" type="xs:string"/>
                                                    <xs:element name="dns2" type="xs:string"/>
                                                    <xs:element name="domainId" type="xs:string"/>
                                                    <xs:element name="domainName" type="xs:string"/>
                                                    <xs:element name="internalDns1" type="xs:string"/>
                                                    <xs:element name="internalDns2" type="xs:string"/>
                                                    <xs:element name="networkType" type="xs:string"/>
                                                    <xs:element name="securityGroupsEnabled" type="xs:boolean"/>
                                                </xs:sequence>
                                            </xs:extension>
                                        </xs:complexContent>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Comment">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information of a comment or question from an user.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="abused" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to determine if this comment has been flagged as abusive (the message content could be inappropriate</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="abuseUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to use to flag that this comment is inappropriate</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="likesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to use to like this comment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="loginName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the username of the user who has posted this comment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="message" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the comment content</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbAbused" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of times this comment has been flagged as inappropriate</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbDislike" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of dislikes for this comment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbLike" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of likes for this comment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="photo" type="Logo" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the avatar/photo of the user who has posted this comment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rating" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the overall rating for this comment (used when this comment is a review)</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:int"/>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="replies" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of replies to this comment (list of :ref:`comment-object` objects)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="comment" type="Comment" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="replyUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to post a reply to this comment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tags" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a string of tags for this comment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the comment type</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="QUESTION"/>
                                <xs:enumeration value="REPLY"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this comment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Comments">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`comment-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="comments" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`comment-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="comment" type="Comment" minOccurs="0" maxOccurs="unbounded"/>
