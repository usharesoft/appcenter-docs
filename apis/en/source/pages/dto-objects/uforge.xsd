                    <xs:element name="publicImage" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating whether this machine image is public, allowing any user to use to provision instances</xsd:documentation>
                        </xsd:annotation>
