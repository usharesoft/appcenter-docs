    <xs:complexType name="Deployment">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides all the information of a deployment. A deployment is a set of one or more running instances (workloads).
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the uri of the deployment</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="applicationId" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the application id of the deployment</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the name of the deployment</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="creationDate" type="xs:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the date of the deployment</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="blueprintName" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the blueprint name used to do this deployment</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="iconUrl" type="xs:anyURI" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the icon url of the application deployed</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="state" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the deployment state</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="sourceSummary" type="SourceSummary" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the deployment source (Blueprint, Appliance or Scan)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:sequence>
                <xs:element name="instances" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">the list of :ref:`instance-object` objects</xsd:documentation>
                    </xsd:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="instance" type="Instance" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Deployments">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`deployment-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="deployments" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`deployment-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="deployment" type="Deployment" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Disk">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information of a physical disk that is part of a partitioning table. A disk
                may also contain physical and logical partitions.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="blockSize" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the disk block size (bytes)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the disk name, for example ``sda``</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="sda"/>
                                <xs:enumeration value="sdb"/>
                                <xs:enumeration value="sdc"/>
                                <xs:enumeration value="sdd"/>
                                <xs:enumeration value="sde"/>
                                <xs:enumeration value="sdf"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="partitions" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of partitions this disk has (see :ref:`partition-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="partition" type="Partition" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="partitionsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the partitions for this disk</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionTableUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the disk type</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="LVM"/>
                                <xs:enumeration value="MSDOS"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="partitionUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uris for the partitions this disk has</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the disk size (in bytes)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource for this disk</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Disks">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`disk-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="disks" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`disk-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="disk" type="Disk" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="DistribProfile" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes an abstract profile shared between linux and windows.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="abbreviation" type="xs:string" minOccurs="0"/>
                    <xs:element name="active" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine if this os profile is active (i.e. available to use by a user)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applianceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the appliance this os profile is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="arch" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the architecture of the operating system this os profile is based on</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description of this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the operating system information this os profile is based on</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installedSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">estimated install disk size required (in bytes) to install the packages this template contains. Note, this attempts to include all package dependencies.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="standardProfileUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get the os profile this profile is based on (see :ref:`linuxprofile-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total size of all the packages contained in this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="DistribProfiles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`distribprofile-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="distribProfiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`distribprofile-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="distribProfile" type="DistribProfile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Distribution">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a specific version of an operating system that is registered on the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="active" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to determine if this operating system is ``active`` i.e. namely can be accessed and used by users</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="arch" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the architecture of this operating system</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="i386"/>
                                <xs:enumeration value="i586"/>
                                <xs:enumeration value="i686"/>
                                <xs:enumeration value="noarch"/>
                                <xs:enumeration value="x86_64"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="baseMilestonesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the milestones for this operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="basePkgsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the packages contained in this operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseProfilesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the os profile templates (see :ref:`distribprofile-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseRepositoriesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the distribution repositories (see :ref:`repository-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="baseProhibitedFormatsUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="baseProhibitedTargetFormatsUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="baseServicesUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="baseGoldensUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the base machine images (for Microsoft Windows only)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="company" type="Company" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the company information of this operating system (see :ref:`company-object`</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="completeProfileUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description of this operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="family" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the family type for this operating system</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="linux"/>
                                <xs:enumeration value="unix"/>
                                <xs:enumeration value="windows"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="lastPkgBuildDate" type="xs:dateTime" minOccurs="0"/>
                    <xs:element name="license" type="License" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the license information for this operating system (see :ref:`license-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="milestoneUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each milestone registered to this operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the operating system</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="CentOS"/>
                                <xs:enumeration value="Debian"/>
                                <xs:enumeration value="Fedora Core"/>
                                <xs:enumeration value="OpenSUSE"/>
                                <xs:enumeration value="Oracle Linux"/>
                                <xs:enumeration value="RedHat Enterprise Linux"/>
                                <xs:enumeration value="Scientific Linux"/>
                                <xs:enumeration value="SUSE Linux Enterprise Server"/>
                                <xs:enumeration value="Ubuntu"/>
                                <xs:enumeration value="Windows"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this operating system is attached to (normally an organization)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="pkgType" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the native package type this operating system supports</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="preselected" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="profiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of os profile template objects (see :ref:`distribprofile-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="profile" type="DistribProfile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="profileUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each os profile template registered for this operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="prohibitedFormats" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="prohibitedFormat" type="ImageFormat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="prohibitedTargetFormats" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="prohibitedTargetFormat" type="TargetFormat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="releaseDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date this operating system was released</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="serviceUris" type="UriList" minOccurs="0"/>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the estimated size of the operating system (total size of all the packages in the repository)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="skeletonSize" type="xs:long" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource for this operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of this operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="versionId" type="xs:long" minOccurs="0"/>
                    <xs:element name="visible" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine if this operating system should be displayed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="welcomeMsg" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Distributions">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`distribution-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="distributions" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`distribution-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="distribution" type="Distribution" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Entitlement">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a base permission tag pre-defined in the platform. These entitlements are
                used to create :ref:`role-object` that can be assigned to users.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="description" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the entitlement description</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the entitlement name</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Entitlements">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`entitlement-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="entitlements" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`entitlement-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="entitlement" type="Entitlement" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="FileEntry">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the meta-data information of a file on a filesystem.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="checksum" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the sha256 checksum value of the file</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="directory" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag determining if this file is a directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fullPath" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the full canonical pathname of the file</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="group" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the group name for this file (access)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="linkTarget" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the destination of a soft or hard link in a filesystem</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this file</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="owner" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the filesystem owner of the file</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this file entry is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="permissions" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the file permissions</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="scannedInstanceId" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the id of the parent scanned instance this object belongs to (see :ref:`scannedinstance-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the size (in bytes) of this file</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="subEntries" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of files if this file entry is a directory</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this file entry object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Galleries">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="galleries" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="gallery" type="Gallery" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Gallery">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="membersUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="name" type="xs:string" minOccurs="0"/>
                    <xs:element name="nbTemplates" type="xs:long" minOccurs="0"/>
                    <xs:element name="orgUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="partnersUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="productsNotificationsUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="publicGallery" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="publishers" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="publisher" type="User" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="publishersUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="templatesNotificationsUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="templatesUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="userGroupsUri" type="xs:anyURI" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="GalleryTemplate">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the information of an appliance template that has been shared with other
                users to use (e.g. shared into a workspace).
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Appliance">
                <xs:sequence>
                    <xs:element name="allowComments" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to determine if users can post comments for this shared appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applianceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the original appliance template that was shared</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <!-- TODO: we should remove this hardcoded list and expose the list of categories in the org -->
                    <xs:element name="category" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the category of this shared appliance template</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Blogging"/>
                                <xs:enumeration value="Bug Tracking"/>
                                <xs:enumeration value="Business Integration"/>
                                <xs:enumeration value="Business Intelligence"/>
                                <xs:enumeration value="Cloud Infrastructure"/>
                                <xs:enumeration value="Collaboration"/>
                                <xs:enumeration value="Communications"/>
                                <xs:enumeration value="Configuration Management"/>
                                <xs:enumeration value="Content Management"/>
                                <xs:enumeration value="CRM"/>
                                <xs:enumeration value="Data Integration"/>
                                <xs:enumeration value="Database"/>
                                <xs:enumeration value="Database Management"/>
                                <xs:enumeration value="Development Tools"/>
                                <xs:enumeration value="Discussion Forums"/>
                                <xs:enumeration value="Document Management"/>
                                <xs:enumeration value="eCommerce"/>
                                <xs:enumeration value="Education"/>
                                <xs:enumeration value="ERP"/>
                                <xs:enumeration value="Games Entertainment"/>
                                <xs:enumeration value="Identity"/>
                                <xs:enumeration value="Information Management"/>
                                <xs:enumeration value="Infrastructure"/>
                                <xs:enumeration value="Instant Messaging"/>
                                <xs:enumeration value="Monitoring"/>
                                <xs:enumeration value="Network Management"/>
                                <xs:enumeration value="Networking"/>
                                <xs:enumeration value="Other"/>
                                <xs:enumeration value="Portal"/>
                                <xs:enumeration value="Project Management"/>
                                <xs:enumeration value="Proxy Server"/>
                                <xs:enumeration value="Reporting"/>
                                <xs:enumeration value="Security"/>
                                <xs:enumeration value="Software Development"/>
                                <xs:enumeration value="System"/>
                                <xs:enumeration value="System Administration"/>
                                <xs:enumeration value="Telephony"/>
                                <xs:enumeration value="Ticket Management"/>
                                <xs:enumeration value="User Management"/>
                                <xs:enumeration value="Web Application Servers"/>
                                <xs:enumeration value="Wiki"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="comments" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of comments for this shared appliance template (see :ref:`comment-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="comment" type="Comment" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="commentsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the comments for this shared appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="galleryUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="nbBootScripts" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of boot scripts registered to this appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbClones" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of times this appliance template has been imported (cloned to another user's appliance library)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbDownloads" type="xs:long" minOccurs="0"/>
                    <xs:element name="nbOSPackages" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of native packages this appliance template has (in the os profile)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbVotes" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of times this appliance template has been rated</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbWatchers" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of users watching this appliance template for updates</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="notificationsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to use to register for notifications when an appliance template has been updated</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="osProfileName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="owner" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the owner of the original appliance template (that was shared)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publisher" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the login name of the user who has shared the appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rating" type="xs:float" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the overall rating of the appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="relatedVersions" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`gallerytemplate-object` objects this appliance template is related to</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="galleryTemplate" type="GalleryTemplate" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="releaseDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the release date of this shared appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="statistics" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the statistics information of this shared appliance template (see :ref:`gallerytemplatestatistics-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="statistic" type="GalleryTemplateStatistics" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="statisticsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get the statitics for this shared appliance template (see :ref:`gallerytemplatestatistics-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="status" type="OpStatus" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current status during the share process (see :ref:`status-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="voteUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to send a rating for this appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="watching" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to determine if the user requesting information on this shared appliance template is currently watching (requesting to be notified on an update) of this shared appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="workspaceName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the workspace where this appliance template is being shared</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="workspaceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the workspace where this appliance template is being shared</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="GalleryTemplates">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`gallerytemplate-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="galleryTemplates" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`gallerytemplate-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="galleryTemplate" type="GalleryTemplate" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="GalleryTemplateStat">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides basic statistics of a :ref:`gallerytemplate-object`. This includes the number of times it has been
                used (imported) by other users
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="downloadId" type="xs:string" minOccurs="0"/>
                    <xs:element name="nbDownloads" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of machine image downloads (generated from this shared appliance template)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbImports" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of imports of this appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object (e.g. workspace)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="user" type="User" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the login name of the user to track actions made on a :ref:`gallerytemplate-object`</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="GalleryTemplateStatistics">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides all the statistical information of a :ref:`gallerytemplate-object`. This includes basic statistics
                (see :ref:`gallerytemplatestat-object`, as well as rating information.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="clonedStats" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`gallerytemplatestat-object` objects holding the statistics describing the number of times an appliance template has been cloned</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="clonedStat" type="GalleryTemplateStat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="clonedStatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all import statistics for an appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="clones" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`statdatapoint-object` providing the history of imports</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="downloads" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`statdatapoint-object` providing the history of download statistics</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="hasRating" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to determine if the appliance template has been rated</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbDownloads" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total number of downloads of machine images related to the appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbImports" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total number of imports for this appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nbVotes" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total number of times this appliance template has been rated</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="overallRating" type="xs:float" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the overall rating of this appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="truncatedDate" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date of the data point</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="voteStats" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of :ref:`votestat-object` providing the all the ratings by users</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="voteStat" type="VoteStat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="voteStatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the ratings by users for this appliance template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="GenerationStage">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data of the generation stage of a migration.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Stage">
                <xs:sequence>
                    <xs:element name="image" type="Image" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the image generated by the generation stage</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imageUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the image generated</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="sourceTemplateId" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the id of the source template if there is one</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Image">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a machine image that can be used to provision one or more instances on a target environment.
                A machine image is created from an appliance template.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeUploadObject">
                <xs:sequence>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this machine image was generated from</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="basePublishImagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the published machine images information (see :ref:`publishimage-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="completeInstalledSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">estimated disk size to install the machine image (in bytes)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="completeSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the size (in bytes) of the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="compress" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to determine if this machine image has been compressed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="credAccount" type="CredAccount" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the cloud account object related to this machine image (see :ref:`credaccount-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the operating system name this machine image is built upon or requires to run</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="downloadKey" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the unique download key to download the machine image without authentication (can only be used once)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="downloadUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to download the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="ebs" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to determine if this machine image should use an EBS volume</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="ebsVolumeSize" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">size of the EBS volume size</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fileExtension" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the file extension of the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fileName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the file name of the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fileSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the download size of the machine image (in bytes)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="format" type="ImageFormat" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the primitive format of this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetFormat" type="TargetFormat" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the external format of this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fullName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the complete file name of the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imageCreated" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date this machine im age was generated</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installProfile" type="InstallProfile" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the install profile information of this machine image (see :ref:`installprofile-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="orgUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the organization where this machine image is stored</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="pkgsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the native packages for this machine image (includes packages added after package dependencies have been calculated)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publicAccess" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="registeredCredAccount" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="revision" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the revision number of this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="simulated" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean to indicate whether a dependency checking simulation has been run to determine the definite list of native packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="statusUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the current generation status</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tag name for this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="updateTools" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean specific for AWS to determine if AWS should have the ability to update its tools automatcially of a provisioned system from this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="vmDiskSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the recommended disk size (in bytes) to use when provisioning an instance from this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="vmMemorySize" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the recommended memory size (in bytes) to use when provisioning an instance from this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="forceCheckingDeps" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to enforce checking dependencies during the generation of this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="entrypoint" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the Entrypoint of a Docker machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="ImageFormat">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the information on a format available for generating a machine image. These are the primitive format types
                supported by the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="access" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to determine if the requesting user has access to this format</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="active" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to determine if this format is visible to the requesting user</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the format</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="aws"/>
                                <xs:enumeration value="azure"/>
                                <xs:enumeration value="cloudburstvmware"/>
                                <xs:enumeration value="cloudstackova"/>
                                <xs:enumeration value="cloudstackqcow2"/>
                                <xs:enumeration value="cloudstackvhd"/>
                                <xs:enumeration value="cloudsigmaraw"/>
                                <xs:enumeration value="cloudwatt"/>
                                <xs:enumeration value="docker"/>
                                <xs:enumeration value="esx"/>
                                <xs:enumeration value="google"/>
                                <xs:enumeration value="hyper-v"/>
                                <xs:enumeration value="hyperv2012"/>
                                <xs:enumeration value="ISO"/>
                                <xs:enumeration value="PXE"/>
                                <xs:enumeration value="kvm"/>
                                <xs:enumeration value="k5vmdk"/>
                                <xs:enumeration value="lxc"/>
                                <xs:enumeration value="openshift"/>
                                <xs:enumeration value="openstackqcow2"/>
                                <xs:enumeration value="openstackvdi"/>
                                <xs:enumeration value="openstackvhd"/>
                                <xs:enumeration value="openstackvmdk"/>
                                <xs:enumeration value="outscale"/>
                                <xs:enumeration value="oracleraw"/>
                                <xs:enumeration value="ovf"/>
                                <xs:enumeration value="ovf09"/>
                                <xs:enumeration value="qcow2"/>
                                <xs:enumeration value="raw"/>
                                <xs:enumeration value="susecloud"/>
                                <xs:enumeration value="targz"/>
                                <xs:enumeration value="vagrant"/>
                                <xs:enumeration value="vbox"/>
                                <xs:enumeration value="vcenter"/>
                                <xs:enumeration value="vcloud"/>
                                <xs:enumeration value="vhd"/>
                                <xs:enumeration value="vmware"/>
                                <xs:enumeration value="xen"/>
                                <xs:enumeration value="xenserver"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="preselected" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this format</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ImageFormats">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`imageformat-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="imageFormats" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`imageformat-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="imageFormat" type="ImageFormat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="ImagePkg">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the details of a group of files (package) that are to be installed as part of a machine image. This may
                includes packages that have not been explicitly set in an appliance template, rather added as part of the generation
                process to adhere to package dependencies.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="level" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">provides the dependency level of this package. For example if this package was added as a dependency of another package, then this will be level 1</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="native_from_distribution" type="xs:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">a boolean determining if this package has come from a registered repository</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="pkg" type="Pkg" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the :ref:`package-object` details</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ImagePkgs">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`imagepkg-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="imagePkgs" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`imagepkg-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="imagePkg" type="ImagePkg" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Images">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`image-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="images" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`image-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="image" type="Image" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="ImageStats">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides statistics for a machine image. This includes the number of times downloaded or registered to a target
                cloud environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="curr" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the value</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="currDownloaded" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">number of downloads of this machine image</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="currFormatUsage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">number of times a specific format has been used</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="currPublished" type="xs:int" minOccurs="0"/>
            <xs:element name="total" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the total value (over time regardless if the object in question has been deleted)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="totalDownloaded" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the total number of times a machine image has been downloaded (even if the referenced machine image has been deleted)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="totalFormatUsage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">list of :ref:`statdatapoint-object` for the breakdown on format types used for generating machine images</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="statDataPoint" type="StatDataPoint" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="totalPublished" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the total number of times this machine image has been published</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="InstallProfile">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes the installation profile of an appliance template. The installation profile holds configuration information
                that should be applied as part of the installation of an operating system. This includes partitioning table layout,
                root password information, kernel parameters, keyboard settings, timezone etc.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="activationKey" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the activation key of the Windows operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applianceUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the appliance template this install profile belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="diskSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the disk size (in bytes).

                                .. note:: this is only used for ``basic partitioning`` with one disk
                            </xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="firewallAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine whether the firewall settings are automatically set, if false this question is asked to the user interactively when provisioning an instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="firewallEnabled" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine if the firewall is enabled (only used if ``firewallAuto = true``)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="groups" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of operating system groups (see :ref:`osgroup-object`</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="group" type="OSGroup" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="groupsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the operating system groups</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="hwType" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the recommended hardware type (used for VMware type machine images)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="internetSettingsAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine whether the internet settings are automatically set, if false this question is asked to the user interactively when provisioning an instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="kernelParameters" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of kernel parameters to use (this is one string with each kernel parameter separated by a comma</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="keyboard" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the keyboard setting (only used if ``keyboardAuto = true``)</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="ar-azerty"/>
                                <xs:enumeration value="ar-azerty-digits"/>
                                <xs:enumeration value="ar-digits"/>
                                <xs:enumeration value="ar-qwerty"/>
                                <xs:enumeration value="ar-qwerty-digits"/>
                                <xs:enumeration value="be-latin1"/>
                                <xs:enumeration value="ben"/>
                                <xs:enumeration value="ben-probhat"/>
                                <xs:enumeration value="bg-bds-utf8"/>
                                <xs:enumeration value="bg-pho-utf8"/>
                                <xs:enumeration value="br-abnt2"/>
                                <xs:enumeration value="cf"/>
                                <xs:enumeration value="croat"/>
                                <xs:enumeration value="cz-lat2"/>
                                <xs:enumeration value="cz-us-qwertz"/>
                                <xs:enumeration value="de"/>
                                <xs:enumeration value="de-latin1"/>
                                <xs:enumeration value="de-latin1-nodeadkeys"/>
                                <xs:enumeration value="dev"/>
                                <xs:enumeration value="dk"/>
                                <xs:enumeration value="dk-latin1"/>
                                <xs:enumeration value="dvorak"/>
                                <xs:enumeration value="es"/>
                                <xs:enumeration value="et"/>
                                <xs:enumeration value="fi"/>
                                <xs:enumeration value="fi-latin1"/>
                                <xs:enumeration value="fr"/>
                                <xs:enumeration value="fr-ch"/>
                                <xs:enumeration value="fr-ch-latin1"/>
                                <xs:enumeration value="fr-latin1"/>
                                <xs:enumeration value="fr-latin9"/>
                                <xs:enumeration value="fr-pc"/>
                                <xs:enumeration value="gr"/>
                                <xs:enumeration value="guj"/>
                                <xs:enumeration value="gur"/>
                                <xs:enumeration value="hu"/>
                                <xs:enumeration value="hu101"/>
                                <xs:enumeration value="ie"/>
                                <xs:enumeration value="is-latin1"/>
                                <xs:enumeration value="it"/>
                                <xs:enumeration value="it-ibm"/>
                                <xs:enumeration value="it2"/>
                                <xs:enumeration value="jp106"/>
                                <xs:enumeration value="ko"/>
                                <xs:enumeration value="la-latin1"/>
                                <xs:enumeration value="mk-utf"/>
                                <xs:enumeration value="nl"/>
                                <xs:enumeration value="no"/>
                                <xs:enumeration value="pl2"/>
                                <xs:enumeration value="pt-latin1"/>
                                <xs:enumeration value="ro"/>
                                <xs:enumeration value="ro-cedilla"/>
                                <xs:enumeration value="ro-std"/>
                                <xs:enumeration value="ro-std-cedilla"/>
                                <xs:enumeration value="ru"/>
                                <xs:enumeration value="sg"/>
                                <xs:enumeration value="sg-latin1"/>
                                <xs:enumeration value="sk-qwerty"/>
                                <xs:enumeration value="sloven"/>
                                <xs:enumeration value="sr-cy"/>
                                <xs:enumeration value="sr-latin1"/>
                                <xs:enumeration value="sv-latin1"/>
                                <xs:enumeration value="tj"/>
                                <xs:enumeration value="tml-inscript"/>
                                <xs:enumeration value="tml-uni"/>
                                <xs:enumeration value="trq"/>
                                <xs:enumeration value="ua-utf"/>
                                <xs:enumeration value="uk"/>
                                <xs:enumeration value="us"/>
                                <xs:enumeration value="us-acentos"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="keyboardAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine whether the keyboard settings are automatically set, if false this question is asked to the user interactively when provisioning an instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="memorySize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the recommended memory size to use for running an instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="nics" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of NICs to configure (see :ref:`nic-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="nic" type="Nic" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="nicsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get the list of NIC information</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine whether to automatically create the partitioning table, if false this question is asked to the user interactively when provisioning an instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionTable" type="PartitionTable" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the partition table information (see :ref:`partitiontable-object`). Only used if ``partitionAuto = true``</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rootUser" type="OSUser" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the root user information, includes root password information</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="runSysprep" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine if sysprep will be executed for Windows operating system</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="skipLicenses" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean to determine whether to display and enforce the user to accept licensing information during the provisioning of an instance. if ``true`` no licensing information is displayed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="swapSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the swap size (in bytes).

                                .. note:: this is only used for ``basic partitioning`` with one disk
                            </xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="timezone" minOccurs="0" type="xs:string">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the timezone to set. Only used if ``timezoneAuto == true``</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="timezoneAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine whether the timezone settings are automatically set, if false this question is asked to the user interactively when provisioning an instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="timezoneCity" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the city name of the timezone that is set (only considered if ``timezoneAuto == true``</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="updateTools" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to determine whether to allow automatic update of AWS libraries and tools in the cloud environment.

                                .. note:: only used for AWS
                            </xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this install profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="users" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of operating system users to set (see :ref:`osuser-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="user" type="OSUser" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="usersUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the operating system users</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="vagrantUser" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user to set for vagrant base boxes</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="welcomeMsg" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">welcome message to display to the end user when provisioning an instance</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="seLinuxMode" type="seLinuxMode" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the SELinux mode (disabled/permissive/enforcing)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xsd:simpleType name="seLinuxMode">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="disabled"/>
            <xsd:enumeration value="permissive"/>
            <xsd:enumeration value="enforcing"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xs:complexType name="Instance">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="hardware" type="xs:string" minOccurs="0"/>
                    <xs:element name="cores" type="xs:int" minOccurs="0"/>
                    <xs:element name="memory" type="xs:int" minOccurs="0"/>
                    <xs:element name="location" type="Location" minOccurs="0"/>
                    <xs:element name="hostname" type="xs:string" minOccurs="0"/>
                    <xs:element name="instanceId" type="xs:string" minOccurs="0"/>
                    <xs:element name="name" type="xs:string" minOccurs="0"/>
                    <xs:element name="creationDate" type="xs:dateTime" minOccurs="0"/>
                    <xs:element name="sourceSummary" type="SourceSummary" minOccurs="0"/>
                    <xs:element name="state" type="xs:string" minOccurs="0"/>
                    <xs:element name="stateMessage" type="xs:string" minOccurs="0"/>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="cloudProvider" type="xs:string" minOccurs="0"/>
                    <xs:element name="sshKeyName" type="xs:string" minOccurs="0"/>
                    <xs:element name="loginUser" type="xs:string" minOccurs="0"/>
                    <xs:element name="winrmAddress" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="InstanceAmazon">
        <xs:complexContent>
            <xs:extension base="Instance">
                <xs:sequence>
                    <xs:element name="instanceType" type="xs:string" minOccurs="0"/>
                    <xs:element name="userName" type="xs:string" minOccurs="0"/>
                    <xs:element name="userPassword" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="InstanceAzure">
        <xs:complexContent>
            <xs:extension base="Instance">
                <xs:sequence>
                    <xs:element name="userName" type="xs:string" minOccurs="0"/>
                    <xs:element name="userPassword" type="xs:string" minOccurs="0"/>
                    <xs:element name="userSshKey" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="InstanceCloudStack">
        <xs:complexContent>
            <xs:extension base="Instance">
                <xs:sequence>
                    <xs:element name="group" type="xs:string" minOccurs="0"/>
                    <xs:element name="offeringName" type="xs:string" minOccurs="0"/>
                    <xs:element name="zoneId" type="xs:string" minOccurs="0"/>
                    <xs:element name="networkId" type="xs:string" minOccurs="0"/>
                    <xs:element name="floatingIpAddress" type="xs:boolean" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="InstanceOpenStack">
        <xs:complexContent>
            <xs:extension base="Instance">
                <xs:sequence>
                    <xs:element name="flavorId" type="xs:string" minOccurs="0"/>
                    <xs:element name="networkId" type="xs:string" minOccurs="0"/>
                    <xs:element name="region" type="xs:string" minOccurs="0"/>
                    <xs:element name="zone" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="InstanceVCloud">
        <xs:complexContent>
            <xs:extension base="Instance">
                <xs:sequence>
                    <xs:element name="description" type="xs:string" minOccurs="0"/>
                    <xs:element name="platformVersion" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Instances">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="instances" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="instance" type="Instance" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="IPAddress">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds IP address information when setting NICs on an appliance template
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="address" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the static IP address to set on the NIC</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="gateway" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the gateway to set on the NIC</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="netmask" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the netmask to set on the NIC</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this object is attached to (e.g. :ref:`nic-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" type="xs:int">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">IP address type (IPv4 or IPv6)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this IP address object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Language">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="displayName" type="xs:string" minOccurs="0"/>
                    <xs:element name="lang" type="xs:string" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="License">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the meta-data of a license file.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeUploadObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the file name of the license</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this license object is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the license type (e.g. ``GPL``, ``GPLv2``, ``CDDL``, ``Apache``)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of thie license object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Licenses">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`license-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="licenses" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`license-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="license" type="License" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LinuxProfile">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a group of native packages from a linux operating system. This profile is
                created by a platform administrator or used in an appliance template. These templates are used by
                users of the platform to easily create operating profiles that are part of an appliance template.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="DistribProfile">
                <xs:sequence>
                    <xs:element name="distributionPackagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the operating system packages contained in this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionServicesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the operating system services contained in this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="excludeDocs" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="packagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the packages this os profile contains (may contain custom packages from other repositories other than the operating system)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="packageUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uris for the packages this os profile contains</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="servicesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the services contained in this os profile</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="serviceUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the available services uri</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="updatesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the available package updates (see :ref:`osupdate-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LinuxProfiles">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`linuxprofile-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="linuxProfiles" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`linuxprofile-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="linuxProfile" type="LinuxProfile" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LocalizedMessage">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Localized message returned by the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="message" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the message in the request language</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="messageId" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the message id. This is a unique id regardless the language. The id will always be the same for the same kind of error.</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="parameters" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the parameters that help a client to rebuild the complete message.</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="parameter" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Location">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Location where a deployed instance is running.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the name of the location</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="provider" type="xs:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the cloud provider</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="credentialsUri" type="xs:anyURI" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the credentials URI used for this location</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="LogicalGroup">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a logical volume group in a partitioning table. A volume group creates a pool of disk space from a
                collection of disks or partitions. This volume group can then be partitioned via a logical volume.

                .. note:: Only disks and partitions of type lvm and logical partitions can be added to a volume group
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="diskUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resources for each disk this logical group uses</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="diskUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each disk this logical group uses</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="groupName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the logical group name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0"/>
                    <xs:element name="partitionTableUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the partition table object (see :ref:`partitiontable-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resources for each partition this logical group uses</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each partition this logical group uses</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this logical group</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LogicalGroups">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`logicalgroup-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="logicalGroups" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`logicalgroup-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="logicalGroup" type="LogicalGroup" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="LogicalVolume">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a logical volume in a partitioning table. A logical volume describes the way a ``volume group`` should
                be partitioned.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="groupUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the logical group this logical volume uses</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this logical volume</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionTableUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the partition table object (see :ref:`partitiontable-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this logical volume object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="volumes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of volumes (partitions), see :ref:`partition-object`</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="volume" type="Partition" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="volumesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the list of volumes (partitions)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="volumeUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources of the volume objects (partitions)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LogicalVolumes">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`logicalvolume-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="logicalVolumes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`logicalvolume-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="logicalVolume" type="LogicalVolume" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Logo">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the meta-data of a logo or photo uploaded onto the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeUploadObject">
                <xs:sequence>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description of this logo</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the file name of the logo</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this logo object belongs</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="title" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tooltip for this logo</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this logo object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Membership">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes the membership status of a user in a :ref:`workspace-object`.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Referral">
                <xs:sequence>
                    <xs:element name="joinedDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date the user accepted the membership invitation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="leftDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date the user has stopped becoming a member or declined the initial membership invitation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="role" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current role of the user (privileges)</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Administrator"/>
                                <xs:enumeration value="Collaborator"/>
                                <xs:enumeration value="Guest"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Memberships">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`membership-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="memberships" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`membership-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="membership" type="Membership" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="msg" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Migration">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data of a migration. Contains the list of stages to perform a migration.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Pipeline">
                <xs:sequence>
                    <xs:element name="cancellable" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean highlighting if the Migration is cancellable or not</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Migrations">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`milestone-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="migrations" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`migration-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="migration" type="Migration" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Milestone">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Marks a specific point along a an operating system's timeline. This can then be used to help users rollback or forward
                package updates of their appliance templates. Milestones are created and maintained by a platform administrator
                for each of the operating systems registered with the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="date" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date for this milestone</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a description of the milestone</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name or title of the milestone</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="source" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the source of the Milestone. Reserved source : distribution (populated automatically by the platform population tools)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this milestone object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Milestones">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`milestone-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="milestones" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`milestone-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="milestone" type="Milestone" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="MySoftware">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a software component contained in a user's ``Software Library``. These software components are private to
                a user, and are maintained by the user. They can be added to a user's appliance templates.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="SoftwareBundle">
                <xs:sequence>
                    <xs:element name="appliancesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the appliance templates that use this software component</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applianceUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources of each appliance template using this software component</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="cloneUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">uri resource used to clone this MySoftware</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imported" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean highlighting if the MySoftware has been imported (from a scan, an appliance import or a workspace)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="generationEnabled" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean highlighting if the Software component is able to be generated or not</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="MySoftwareList">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`mysoftware-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="mySoftwareList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`mysoftware-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="mySoftware" type="MySoftware" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Nic">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the information of a network interface card (NIC). This NIC information is attached to the installation profile belonging to an appliance template.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="autoConnect" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean highlighting if the network card should be activated automatically</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="ipAddresses" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of static IP address information for this NIC (see :ref:`ipaddress-object`</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="ipAddress" type="IPAddress" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="ipv4Method" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the IPv4 method to be used (manual, dhcp, etc.)</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="AUTOMATIC"/>
                                <xs:enumeration value="MANUAL"/>
                                <xs:enumeration value="DISABLED"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="ipv6Method" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the IPv6 method to be used (manual, dhcp, etc.)</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="AUTOMATIC"/>
                                <xs:enumeration value="MANUAL"/>
                                <xs:enumeration value="IGNORE"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of this NIC</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this NIC is attached to (e.g. an install profile object)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the type of NIC</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="ETHERNET"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="order" type="xs:int" nillable="true">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the order of NIC</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this NIC</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Nics">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`nic-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="nics" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`nic-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="nic" type="Nic" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="OASPkg">
        <xs:complexContent>
            <xs:extension base="Pkg">
                <xs:sequence>
                    <xs:element name="license" type="License" minOccurs="0"/>
                    <xs:element name="licenseUploaded" type="xs:boolean" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OpenstackEntity" abstract="true">
        <xs:complexContent>
            <xs:extension base="CloudResources">
                <xs:sequence>
                    <xs:element name="id" type="xs:string" minOccurs="0"/>
                    <xs:element name="regionName" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OpenstackResources">
        <xs:complexContent>
            <xs:extension base="CloudResources">
                <xs:sequence>
                    <xs:element name="tenants" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="tenant" minOccurs="0" maxOccurs="unbounded">
                                    <xs:complexType>
                                        <xs:complexContent>
                                            <xs:extension base="CloudResources">
                                                <xs:sequence>
                                                    <xs:element name="available" type="xs:boolean" minOccurs="0"/>
                                                    <xs:element name="description" type="xs:string" minOccurs="0"/>
                                                    <xs:element name="id" type="xs:string" minOccurs="0"/>
                                                    <xs:element name="regionsEntities" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="regionEntities" minOccurs="0" maxOccurs="unbounded">
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="regionName" type="xs:string" minOccurs="0"/>
                                                                            <xs:element name="flavors" minOccurs="0">
                                                                                <xs:complexType>
                                                                                    <xs:sequence>
                                                                                        <xs:element name="flavor" minOccurs="0" maxOccurs="unbounded">
                                                                                            <xs:complexType>
                                                                                                <xs:complexContent>
                                                                                                    <xs:extension base="OpenstackEntity">
                                                                                                        <xs:sequence>
                                                                                                            <xs:element name="cpu" type="xs:int" minOccurs="0"/>
                                                                                                            <xs:element name="disk" type="xs:int" minOccurs="0"/>
                                                                                                            <xs:element name="ram" type="xs:int" minOccurs="0"/>
                                                                                                        </xs:sequence>
                                                                                                    </xs:extension>
                                                                                                </xs:complexContent>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                    </xs:sequence>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                            <xs:element name="images" minOccurs="0">
                                                                                <xs:complexType>
                                                                                    <xs:sequence>
                                                                                        <xs:element name="image" minOccurs="0" maxOccurs="unbounded">
                                                                                            <xs:complexType>
                                                                                                <xs:complexContent>
                                                                                                    <xs:extension base="OpenstackEntity">
                                                                                                        <xs:sequence>
                                                                                                            <xs:element name="baseImageId" type="xs:string" minOccurs="0"/>
                                                                                                            <xs:element name="description" type="xs:string" minOccurs="0"/>
                                                                                                            <xs:element name="distribArch" type="xs:string" minOccurs="0"/>
                                                                                                            <xs:element name="distribName" type="xs:string" minOccurs="0"/>
                                                                                                            <xs:element name="distribVersion" type="xs:string" minOccurs="0"/>
                                                                                                            <xs:element name="minDisk" type="xs:int" minOccurs="0"/>
                                                                                                            <xs:element name="minRam" type="xs:int" minOccurs="0"/>
                                                                                                            <xs:element name="progress" type="xs:int" minOccurs="0"/>
                                                                                                            <xs:element name="snapshot" type="xs:boolean" minOccurs="0"/>
                                                                                                            <xs:element name="status" type="xs:string" minOccurs="0"/>
                                                                                                        </xs:sequence>
                                                                                                    </xs:extension>
                                                                                                </xs:complexContent>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                    </xs:sequence>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                            <xs:element name="instances" minOccurs="0">
                                                                                <xs:complexType>
                                                                                    <xs:sequence>
                                                                                        <xs:element name="instance" minOccurs="0" maxOccurs="unbounded">
                                                                                            <xs:complexType>
                                                                                                <xs:complexContent>
                                                                                                    <xs:extension base="OpenstackEntity">
                                                                                                        <xs:sequence>
                                                                                                            <xs:element name="flavorId" type="xs:string" minOccurs="0"/>
                                                                                                            <xs:element name="imageId" type="xs:string" minOccurs="0"/>
                                                                                                            <xs:element name="status" type="xs:string" minOccurs="0"/>
                                                                                                        </xs:sequence>
                                                                                                    </xs:extension>
                                                                                                </xs:complexContent>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                    </xs:sequence>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                            <xs:element name="networks" minOccurs="0">
                                                                                <xs:complexType>
                                                                                    <xs:sequence>
                                                                                        <xs:element name="network" minOccurs="0" maxOccurs="unbounded">
                                                                                            <xs:complexType>
                                                                                                <xs:complexContent>
                                                                                                    <xs:extension base="CloudResources">
                                                                                                        <xs:sequence>
                                                                                                            <xs:element name="id" type="xs:string" minOccurs="0"/>
                                                                                                        </xs:sequence>
                                                                                                    </xs:extension>
                                                                                                </xs:complexContent>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                    </xs:sequence>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:extension>
                                        </xs:complexContent>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="OpStatus">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Primitive object to describe the status of the parent object it is attached to. Normally used for asynchronous
                actions on the platform (e.g. generating machine images, registering machine images, importing/exporting appliance
                templates).
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="bytesLoaded" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the number of bytes uploaded or downloaded (used for calculating progress)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="bytesTotal" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total number of bytes expecting to be loaded (used for calculating progress)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="cancelled" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to indicate that the job this status was attached to has been cancelled</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="complete" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to indicate that the job this status was attached has completed</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="detailedError" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to indicate that there is detailed error message information</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="detailedErrorMsg" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the detailed error message</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="error" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to indicate that an error has occurred in the job this status is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="errorMessage" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the error message</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="eta" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the estimated time of when the job will complete</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="message" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current status message</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this status object is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="percentage" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the current progress as a percentage</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="published" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag used to indicate if the job has finished (for publishing jobs)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="totalTime" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total time the job has taken to complete</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this status object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OpStatuses">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`status-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="statuses" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`status-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="status" type="OpStatus" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Org">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the meta-data of an organization of the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="activateNewUsers" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to indicate to make any news users being created in this organization to automatically active</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="admin" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to indicate whether the user requesting this organization information is an administrator of this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="appliancesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the appliance templates in this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="applianceUris" type="UriList" minOccurs="0"/>
                    <xs:element name="categoriesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the categories added to this organization (see :ref:`category-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="companiesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the companies registered in this organization (see :ref:`company-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="company" type="Company" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the org company</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="defaultOrg" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate if this organization is the default of the platform</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the operating systems of the organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetFormats" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of target formats available in this organization</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="targetFormat" type="TargetFormat" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="formatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the available primitive formats in this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetPlatformsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the available target platforms in this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetFormatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the available target formats in this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="galleryUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="license" type="License" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the org license</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="licensesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the registered licenses in the org</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="licenseUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the license of the org</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="membersUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the users registered to this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="milestonesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the milestones registered to this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the organization name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="projectsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the projects registered to this organization's project catalog</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="rolesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the roles registered to this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="statsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the statistics for this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="subscriptionProfilesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the subscription profiles registered to this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource for this organization object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="workspacesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the workspaces registered to this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="repositoriesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the operating system repositories registered to this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="artifactAccountsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the artifact accounts registered to this organization</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Orgs">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`org-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="orgs" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`org-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="org" type="Org" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OsConfig">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides OS configuration information
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="edition" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">OS edition</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="DATACENTER"/>
                                <xs:enumeration value="STANDARD"/>
                                <xs:enumeration value="ENTERPRISE"/>
                                <xs:enumeration value="WEBSERVER"/>
                                <xs:enumeration value="UNKNOWN"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">OS type</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="CORE"/>
                                <xs:enumeration value="FULL"/>
                                <xs:enumeration value="UNKNOWN"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="language" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">OS language</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OsConfigs">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`osconfig-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="osConfigs" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`osconfig-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="osConfig" type="OsConfig" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="OSGroup">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Description of an operating system group for filesystem directory and file permissions.
                See also :ref:`osuser-object`.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="groupId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the unique id of the group (only used if ``groupIdAuto == false``</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="groupIdAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to determine if the group id should be calculated</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the group name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="sudo" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a flag to configure sudoers parameters (0 : no sudo, 1: sudo, 2: sudo without password)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="systemGroup" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">flag to determine if this is a system group</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this group object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="password" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the group password</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="encrypted" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean flag to determine if the group password string is encrypted</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OSGroups">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`osgroup-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="osGroups" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`osgroup-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="osGroup" type="OSGroup" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OSPackage">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds information of a native operating system package attached to a software bundle.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="SoftwareArtifact">
                <xs:sequence>
                    <xs:element name="sticky" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate that this package version should not be updated when new package updates are available</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="repoId" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the unique id of the repository these packages belong to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="repository" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">repository name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distroId" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the unique id of the operating system this software artifact (OS Package) belong to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OSPackages">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`ospackage-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="osPkgs" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="osPkg" type="OSPackage" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OSUpdate">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the information of a new package available in the an operating system repository compared with an
                existing package in an appliance template's ``os profile``.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="currPkg" type="Pkg" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the current package information (see :ref:`package-object`)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="date" type="xs:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the end date where to calculate the available updates for the ``currPkg``</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="newPkg" type="Pkg" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the proposed new package version to update to</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="numUpdates" type="xs:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the number of updates available for this package from the current package version to the ``date`` value</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="pkgs" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of package updates available between ``currPkg`` version and the ``date`` provided</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="pkg" type="Pkg" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="OSUpdates">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the operating system updates available for an appliance template. This is the result of comparing the
                appliance template's ``os profile`` package list with the operating system repository registered on the platform.
                Versions of each package are compared, all new package versions are provided in this object.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="availUpdates" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of package updates (see :ref:`osupdate-object`) available for the list of packages in the ``os profile``</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="update" type="OSUpdate" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="osEOLDate" type="xs:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the date where the operating system will receive no more updates</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="osReleaseDate" type="xs:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the date where the operating system was released</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="simulation" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of package updates (see :ref:`osupdate-object`) available after a simulation</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="update" type="OSUpdate" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="simulationDate" type="xs:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the end date where to calculate the available updates (for a simulation)</xsd:documentation>
                </xsd:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="OSUser">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Description of an operating system user for filesystem directory and file permissions. OS users can be grouped
                together using :ref:`osgroup-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="disablePasswordLogin" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to disable the user logging into the system using a password</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="encrypted" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating that the password string is encrypted</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fullName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the full name of the user</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="homeDir" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the home directory of the user</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="password" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user password</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="passwordAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating for the platform to automatically set the password listed in the attribute password</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="primaryGroup" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the user's primary group (see :ref:`osgroup-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="primaryGroupAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating for the platform to automatically assign the user's primary group</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="secondaryGroups" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of the user's secondary groups (each group separated by a comma)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="shell" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the shell type for the user</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="sshKeys" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a list of public SSH keys for the user (see :ref:`sshkey-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="sshKey" type="SSHKey" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="sudo" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a flag to configure sudoers parameters (0 : no sudo, 1: sudo, 2: sudo without password)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="systemUser" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean indicating that this is a system user</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this user object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="userId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the unique id of this user (only used if ``userIdAuto == false``</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="userIdAuto" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating whether the platform should automatically calculate the user's id</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OSUsers">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`osuser-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="osUsers" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`osuser-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="osUser" type="OSUser" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Partition">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information of a physical or logical partition that is part of a partitioning table.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="defaultSubvolume" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating whether this partition is default subvolume</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="dumpLocation" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the location of the partition's dump file</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fstype" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the filesystem type of the partition</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="btrfs"/>
                                <xs:enumeration value="ext2"/>
                                <xs:enumeration value="ext3"/>
                                <xs:enumeration value="ext4"/>
                                <xs:enumeration value="Extended"/>
                                <xs:enumeration value="fat16"/>
                                <xs:enumeration value="fat32"/>
                                <xs:enumeration value="jfs"/>
                                <xs:enumeration value="linux-swap"/>
                                <xs:enumeration value="lvm2"/>
                                <xs:enumeration value="ntfs"/>
                                <xs:enumeration value="unformatted"/>
                                <xs:enumeration value="xfs"/>
                                <xs:enumeration value="reserved"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="growable" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating whether this partition is growable</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="label" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">partition label</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logicalPartitions" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">list of logical partitions (this needs to be an ``extended`` partition)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="logicalPartition" type="Partition" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="logicalPartitionsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the logical partitions</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logicalPartitionUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uris of each logical partition</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="mpoint" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the mount point</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the partition name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this partition is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="partitionTableUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the size (in bytes) of the partition</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this partition object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="volumeOrder" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the volume order</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="mbrBoot" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating whether this partition is bootable</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Partitions">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`partition-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="partitions" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`partition-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="partition" type="Partition" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="PartitionTable">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a complete partitioning table for an appliance template. This partitioning table is part of the
                :ref:`installprofile-object`.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="btrfsStoragePools" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of btrfs storage pools for this partition table (see :ref:`btrfsstoragepool-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="btrfsStoragePool" type="BtrfsStoragePool" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="btrfsStoragePoolsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the btrfs storage pools in this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="btrfsStoragePoolUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each btrfs storage pool in this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="btrfsSubvolumes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of btrfs subvolumes (partitions) in this partition table</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="btrfsSubvolume" type="BtrfsSubvolume" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="btrfsSubvolumesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the btrfs subvolumes in this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="btrfsSubvolumeUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each btrfs subvolume in this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="disks" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of disks this partition table has (see :ref:`disk-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="disk" type="Disk" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="disksUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the disks for this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="diskUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uris for each disk of the partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent this partition table is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logicalGroups" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of logical groups for this partition table (see :ref:`logicalgroup-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="logicalGroup" type="LogicalGroup" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="logicalGroupsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the logical groups in this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logicalGroupUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each logical group in this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logicalVolumes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of logical volumes (partitions) in this partition table</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="logicalVolume" type="LogicalVolume" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="logicalVolumesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the logical volumes in this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="logicalVolumeUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each logical volume in this partition table</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this partition table object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PCpu">
        <xs:sequence>
            <xs:element name="frequency" type="xs:string" minOccurs="0"/>
            <xs:element name="nb_cores" type="xs:string" minOccurs="0"/>
            <xs:element name="usage" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Pipeline">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data of a pipeline. Contains the list of stages to perform a pipeline.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="tag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tag information</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the pipeline name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="status" type="OpStatus" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the status of the pipeline</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of the pipeline</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="stages" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of stages to perform the pipeline (see :ref:`stage-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="stage" type="Stage" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Pipelines">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`pipeline-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="pipelines" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`pipeline-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="pipeline" type="Pipeline" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Pkg">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds information of a native operating system package or file.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeUploadObject">
                <xs:sequence>
                    <xs:element name="arch" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the architecture type for this package</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="binaryUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the binary</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="default" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="extract" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to extract an archive (if the uploaded object is an archive) as part of the generation</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="fullName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the file including the extension</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="install" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate whether to install the package or just to store it on the filesystem</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installLocation" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the location to store the package if ``install == false``</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="language" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the package</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="origName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the original name of the package</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="params" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">silent install parameters to use as part of the installation of the object (for windows only)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this package belongs to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="pkgBuildDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the date the package was built</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="pkgFileDate" type="xs:dateTime" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the file timestamp</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="release" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the release information package</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="shortTag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">tag value</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="repositoryName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">repository name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="sticky" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate that this package version should not be updated when new package updates are available</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">file media type</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this package object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version of this package</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="epoch" type="xs:int" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the epoch value (used for native packages)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PkgGroup">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Describes a group of packages.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="category" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the category for this group of packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="company" type="Company" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the company name</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the description for this group of packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distributionUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the operating system these packages belong to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distroId" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the unique id of the operating system these packages belong to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="installedSize" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the estimated disk size (in bytes) required to install these group of packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="license" type="License" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the license information (see :ref:`license-object`)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of these group of packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="packagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve the details of these packages</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="packageUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources for each package in the group</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="release" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the release information for the group</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="restricted" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="size" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the total size of all the packages in the group (in bytes)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tag" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tag information</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this package group object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="version" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the version information of this package group</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="packages" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of packages in this group (see :ref:`package-object`)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="pkg" type="Pkg" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PkgList">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`package-object` objects to add or delete.
            </xsd:documentation>
        </xsd:annotation>
        <xs:sequence>
            <xs:element name="addedPkgUris" type="UriList" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of :ref:`package-object` uris to add</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="deletedPkgUris" type="UriList" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of :ref:`package-object` uris to remove</xsd:documentation>
                </xsd:annotation>
            </xs:element>
            <xs:element name="pkgs" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">the list of :ref:`package-object` objects</xsd:documentation>
                </xsd:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="pkg" type="Pkg" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="PkgUpdateInfo">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="date" type="xs:dateTime" minOccurs="0"/>
                    <xs:element name="description" type="xs:string" minOccurs="0"/>
                    <xs:element name="source" type="xs:string" minOccurs="0"/>
                    <xs:element name="sourceId" type="xs:string" minOccurs="0"/>
                    <xs:element name="type" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PkgUpdateInfoList">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="pkgUpdateInfoList" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="pkgUpdateInfo" type="PkgUpdateInfo" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PRam">
        <xs:sequence>
            <xs:element name="quantity" type="xs:string" minOccurs="0"/>
            <xs:element name="usage" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ProductAccess">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="allowed" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="cryptAlgo" type="xs:int" minOccurs="0"/>
                    <xs:element name="email" type="xs:string" minOccurs="0"/>
                    <xs:element name="expires" type="xs:dateTime" minOccurs="0"/>
                    <xs:element name="licenseContents" type="xs:string" minOccurs="0"/>
                    <xs:element name="licenseKey" type="xs:string" minOccurs="0"/>
                    <xs:element name="machineId" type="xs:string" minOccurs="0"/>
                    <xs:element name="type" type="xs:string" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Project">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                A software component that is part of the platform's ``Project Catalog``. Such software components are public for
                all users to use in their appliance templates. These software components are updated and maintained by an
                administrator of the platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="SoftwareBundle">
                <xs:sequence>
                    <xs:element name="orgUris" type="UriList" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of uri resources of each organization this software bundle (project) has been registered to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Projects">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`project-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="projects" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`project-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="project" type="Project" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PublicationStage">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data of the publication stage of a migration.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Stage">
                <xs:sequence>
                    <xs:element name="publishImage" type="PublishImage" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the image published by the publication stage</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publishImageUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the published image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="sourceImageId" type="xs:long" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the id of the source image if there is one</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PublishImage" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the general information on the registration of a machine image to a target cloud environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="Image">
                <xs:sequence>
                    <xs:element name="cloudId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the unique id for the machine image registered on the cloud environment</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="imageUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="instances" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="instance" type="Instance" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="instancesUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="publishLocation" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the location of the registered machine image (e.g. region)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PublishImageAws">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information on the registration of a machine image to the Amazon Web Services (AWS) environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="region" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the region where this machine image is registered</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PublishImageAzure">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information on the registration of a machine image to the Microsoft Azure environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="storageAccount" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the storageAccount where this machine image is registered</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="container" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the container where this machine image is registered</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="resourceGroup" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the resourceGroup where this machine image is registered</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="blob" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the blob of the published machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="displayName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the cloud name for this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="PublishImageCloudStack">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information on the registration of a machine image to the CloudStack environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the public description of this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="displayName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the public name displayed for this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="featuredEnabled" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate whether this machine image is ``featured``</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="passwordEnabled" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate whether a password has been enabled for this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publicImage" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean to indicate whether this machine image can be used publicly by all users of the CloudStack platform</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="zoneName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the zone where this machine image is registered</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PublishImageCloudwatt">
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="displayName" type="xs:string" minOccurs="0"/>
                    <xs:element name="keystoneDomain" type="xs:string" minOccurs="0"/>
                    <xs:element name="keystoneProject" type="xs:string" minOccurs="0"/>
                    <xs:element name="publicImage" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="tenantName" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PublishImageGoogle">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information on the registration of a machine image to the Google Compute Engine (GCE) environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="bucket" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the bucket name where this machine image is stored</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="bucketLocation" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the location of the bucket where this machine image is stored</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="diskNamePrefix" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the disk name prefix used when registering the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="description" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the public description of the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="projectId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the project id used for publishing this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="storageClass" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the storage class used for publishing this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="zoneName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the zone name used for publishing this machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PublishImageOpenStack">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information on the registration of a machine image to the OpenStack environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="keystoneDomain" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the keystone domain to use when registering the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="keystoneProject" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the keystone project to use when registering the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="displayName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the public name of the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="publicImage" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">boolean indicating whether this machine image is public, allowing any user to use to provision instances</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="tenantName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the tenant name to use when registering the machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="PublishImageOutscale">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information on the registration of a machine image to the Outscale environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="bucket" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the bucket used to store the registered machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="region" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the region used to store the registered machine image</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="PublishImageSuseCloud">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information on the registration of a machine image to the SuseCloud environment.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="PublishImage">
                <xs:sequence>
                    <xs:element name="keystoneDomain" type="xs:string" minOccurs="0">
