    <xs:complexType name="CredAccountOpenStack">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds specific account information for the OpenStack cloud platform.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="CredAccount">
                <xs:sequence>
                    <xs:element name="glanceUrl" type="xs:anyURI" minOccurs="0">
