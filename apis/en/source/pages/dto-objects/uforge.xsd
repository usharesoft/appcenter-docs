    <xs:complexType name="ConfMgmtPlatform">
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="enabled" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0"/>
                    <xs:element name="project" type="Project" minOccurs="0"/>
                    <xs:element name="type" type="xs:string" minOccurs="0"/>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ConfMgmtPlatforms">
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="platforms" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="confMgmtPlatforms" type="ConfMgmtPlatform" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ContainerTemplate">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds all the meta-data of a containerTemplate.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of this container template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="softwareBundle" type="SoftwareBundle" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the software bundle from which the container template has been created</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="distribution" type="Distribution" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the distribution on which the container template is based</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="images" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of images generated from the container template</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="image" type="Image" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="imagesUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri to retrieve images generated from the container template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="formatsUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the target formats compatible with this container template</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ContainerTemplates">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides a list of :ref:`ContainerTemplate-object` objects.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObjects">
                <xs:sequence>
                    <xs:element name="containerTemplates" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of :ref:`ContainerTemplate-object` objects</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="containerTemplate" type="ContainerTemplate" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CredAccount" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds general account information for a target cloud environment. These credentials allow the platform
                to communicate with the target cloud environment including registering machine images.
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the cloud account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this cloud account is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this cloud account</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="targetPlatform" type="TargetPlatform" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the target platform (see :ref:`targetplatform-object`) this cloud account is for</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="secretsMissing" type="xs:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">a boolean flag to indicate that the secrets of this credAccount are missing and left empty in this object</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CredAccountAws">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds specific account information for the Amazon Web Services (AWS) cloud platform.
            </xsd:documentation>
        </xsd:annotation>
