                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the access key to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="accountNumber" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the account number to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="keyPairName" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the key pair name to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="secretAccessKeyId" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the secret access key to use</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CredAccountResource">
        <xs:sequence>
            <xs:element name="key" type="xs:string" minOccurs="0"/>
            <xs:element name="type" type="xs:string" minOccurs="0"/>
            <xs:element name="value" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CredAccountResources">
        <xs:sequence>
