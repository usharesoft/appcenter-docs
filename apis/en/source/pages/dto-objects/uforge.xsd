    <xs:complexType name="Category">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Holds the information of one category available on the platform. Categories are used
                to classify appliance templates into groups. A category may have child categories, allowing an administrator
                to create more complicated taxonomy
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeObject">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the category name (displayed name)</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="orgUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the organization this category is registered to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="subCategories" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the list of sub categories (list of :ref:`category-object` objects)</xsd:documentation>
                        </xsd:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="category" type="Category" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="subCategoryUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to get all the sub-categories</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri of this category</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the category type</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="PROJECT"/>
                                <xs:enumeration value="TEMPLATE"/>
                                <xs:enumeration value="IMAGEFORMAT"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="Certificate">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Provides the information of a certificate file. Certificates are used as part of some cloud accounts
            </xsd:documentation>
        </xsd:annotation>
        <xs:complexContent>
            <xs:extension base="UForgeUploadObject">
                <xs:sequence>
                    <xs:element name="content" type="xs:base64Binary" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the certificate file content</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="name" type="xs:string" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the name of the certificate file</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="parentUri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of the parent object this certificate is attached to</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                    <xs:element name="type" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the certificate file type</xsd:documentation>
                        </xsd:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="ec2KeyPairPrivateKey"/>
                                <xs:enumeration value="ec2PrivateKey"/>
                                <xs:enumeration value="googleCertificate"/>
                                <xs:enumeration value="uForgeCertificate"/>
                                <xs:enumeration value="x509"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="uri" type="xs:anyURI" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource of this certificate</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

