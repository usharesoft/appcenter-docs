                        <xsd:annotation>
                            <xsd:documentation xml:lang="en">the uri resource to retrieve all the operating system this bundle restriction is for</xsd:documentation>
                        </xsd:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="BundleRestrictions">
